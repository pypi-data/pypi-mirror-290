    "recipeIngredient": [
        "1 (16 ounce) package Jimmy DeanÂ® Premium Pork Regular Sausage",
        "1 (8 ounce) package cream cheese, softened",
        "4 ounces garden vegetable cream cheese, softened",
        "1 cup shredded Cheddar cheese",
        "0.25 cup finely chopped red bell pepper",
        "0.25 cup finely chopped green onion",
        "2 (8 ounce) packages crescent roll sheets"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 375 degrees F (190 degrees C)."
        },
        {
            "@type": "HowToStep",
            "text": "Add sausage to a frying pan over medium-high heat. Cook and stir with a spatula, breaking apart sausage until cooked, 5 to 7 minutes. Drain and let cool."
        },
        {
            "@type": "HowToStep",
            "text": "Combine regular and vegetable cream cheese, Cheddar cheese, bell pepper, green onion, and cooled, crumbled sausage in a bowl. Mix well and set aside."
        },
        {
            "@type": "HowToStep",
            "text": "Unroll 1 crescent dough sheet and press it into a 9x12-inch rectangle. Cut 6 rows across and 4 rows vertically to yield 24 squares. Repeat with second package of dough."
        },
        {
            "@type": "HowToStep",
            "text": "Place 1 square in each cup of two nonstick 24-cup mini muffin tins, pressing it into the bottom and up the sides."
        },
        {
            "@type": "HowToStep",
            "text": "Drop 1 heaping teaspoon of sausage filling into each cup. Filling should be up to the top of the cup, but you do not need to press it down."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in the preheated oven until dough is golden brown, 11 to 13 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Cool for 5 minutes in the tins before removing to a platter."
        }
    ],
    "recipeYield": [
        "48"
    ],
