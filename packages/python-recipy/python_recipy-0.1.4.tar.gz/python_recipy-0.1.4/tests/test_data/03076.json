    "recipeIngredient": [
        "1 (10 ounce) package refrigerated biscuit dough",
        "8 slices American processed cheese",
        "8 frankfurters"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/1NCmdTAv6v8M08ewHaIidFdorU8=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21780-pigs-in-a-blanket--ddmfs-Step2-5216-f52dd72a439e46d5a1266b9e414d70d1.jpg"
                }
            ],
            "text": "Shape biscuit dough sections into 3-inch diameter circles. Lay a slice of cheese on each dough circle; place a frankfurter in the center of each cheese slice and roll it up."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/pBsC0Mz2b16aWppj2-qSCqDEfyc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21780-pigs-in-a-blanket--ddmfs-Step3-5220-29da521b80d247978a0ce48d9a3b279c.jpg"
                }
            ],
            "text": "Place rolls, seam-side down, on a cookie sheet."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/WPMIb6gXb9Qt9HE-ahHUdmZXk9s=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21780-pigs-in-a-blanket--ddmfs-Step4-5229-758f031668464ca4b81d94531d2baa52.jpg"
                }
            ],
            "text": "Bake in the preheated oven until biscuits are golden brown, about 10 to 15 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/VJvNav0z_DJootW3Nglc5V21M3k=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21780-pigs-in-a-blanket--ddmfs-3x4-5234-049b676acad340f9a98320f74d409341.jpg"
                }
            ],
            "text": "Serve hot."
        }
    ],
    "recipeYield": [
        "6"
    ],
    "totalTime": "PT30M",
    "review": [
        {
            "@type": "Review",
