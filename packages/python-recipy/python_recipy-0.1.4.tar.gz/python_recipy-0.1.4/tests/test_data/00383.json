    "recipeIngredient": [
        "4 cups cranberries, coarsely chopped",
        "3 cups white sugar",
        "2 cups gin",
        "1 teaspoon vanilla extract"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine cranberries, sugar, gin, and vanilla extract in a large sterilized jar with a tight-fitting lid. Let stand at room temperature for 6 weeks, shaking at least once a week. Pour mixture through a sieve and discard solids. Store in an airtight container for up to 2 months."
        }
    ],
