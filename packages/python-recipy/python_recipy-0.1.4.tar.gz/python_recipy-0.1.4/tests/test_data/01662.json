    "recipeIngredient": [
        "1 1/2 teaspoons matcha powder",
        "1 tablespoon hot water",
        "2 teaspoons honey, or to taste",
        "3/4 cup hot milk"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.foodandwine.com/thmb/s2c2nMJvbeMO4wNRrt3i6PLaqEE=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/FAW-recipes-matcha-latte-ingredients-46661468862c4d4b94f3e2ea94a2c1b8.jpg"
                }
            ],
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.foodandwine.com/thmb/qc2BevK-miODa4OxSJqrhvpNS_U=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/FAW-recipes-matcha-latte-step-01-74d7330be0594043a9a5aee174374825.jpg"
                }
            ],
            "text": "Sift the matcha powder into a mug."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.foodandwine.com/thmb/NCim6rXTQSZX9cktAogIr8ebs_Q=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/FAW-recipes-matcha-latte-step-02-5f1b817fe7fa41099db5b24b3e73018f.jpg"
                }
            ],
            "text": "Add the hot water and whisk until no lumps remain. Stir in the honey."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.foodandwine.com/thmb/5tgBWed0sGOHbA3Nd8psgrainbQ=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/Matcha-Latte-FT-RECIPE0622-6882b11559a3400fbf76fdbb82f3974e.jpg"
                }
            ],
            "text": "Add the milk and either whisk to combine or use a milk frother; serve immediately."
        }
    ],
