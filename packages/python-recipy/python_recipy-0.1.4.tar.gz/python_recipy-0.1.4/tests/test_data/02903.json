    "recipeIngredient": [
        "1/2 lb. Linguini",
        "Kosher salt&nbsp;and freshly ground black pepper",
        "6 oz. unsliced pancetta (or 6 slices thick-cut bacon), diced",
        "1 medium sweet onion, thinly sliced",
        "1 lb. Brussels sprouts, sliced",
        "2 large garlic&nbsp;cloves, finely chopped",
        "1/3 c. dry white wine",
        "3 oz. Parmesan, grated (about 3/4 cup), plus more, shaved, for serving"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Cook pasta in boiling salted water according to package directions; reserve 1 cup pasta water. Drain."
        },
        {
            "@type": "HowToStep",
            "text": "While pasta is cooking: Sauté pancetta and onion in a large deep skillet over medium heat, stirring occasionally, until pancetta is browned and onions are caramelized, 8 to 10 minutes. Add Brussels sprouts and garlic and sauté, stirring, until leaves are bright green, 2 minutes. Stir in wine, scraping brown bits from bottom of skillet. Add pasta, grated Parmesan, and 1/2 to 3/4 cup reserved pasta water, and stir until Parmesan is melted and pasta is creamy and well coated. Season with salt and pepper."
        },
        {
            "@type": "HowToStep",
            "text": "Serve immediately sprinkled with pepper and shaved Parmesan."
        }
    ],
