    "recipeIngredient": [
        "8 large eggs",
        "0.5 cup reduced-fat milk",
        "0.25 teaspoon salt",
        "0.25 teaspoon ground pepper",
        "0.75 cup chopped ham",
        "0.75 cup chopped broccoli",
        "6 tablespoons shredded Cheddar cheese",
        "0.25 cup chopped fresh chives"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 325 degrees F."
        },
        {
            "@type": "HowToStep",
            "text": "Whisk eggs, milk, salt and pepper in a large bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Liberally coat a 12-cup muffin pan with cooking spray (or use silicone muffin cups). Divide ham, broccoli, Cheddar and chives among the muffin cups. Top with the egg mixture. Bake until set and lightly brown, 20 to 25 minutes. Let stand for 5 minutes before removing from the pan."
        }
    ],
