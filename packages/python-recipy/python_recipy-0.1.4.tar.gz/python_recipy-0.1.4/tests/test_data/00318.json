    "recipeIngredient": [
        "4 egg whites",
        "5 cups confectioners' sugar",
        "2 tablespoons ground ginger",
        "1 tablespoon ground cinnamon",
        "2 teaspoons vanilla extract"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Beat egg whites in a bowl using an electric mixer on high speed until foamy. Gradually mix confectioners&#39; sugar, ginger, cinnamon, and vanilla extract into egg whites until icing is thickened."
        }
    ],
