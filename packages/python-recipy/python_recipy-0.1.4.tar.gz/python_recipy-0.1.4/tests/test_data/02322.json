    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Spray 2 1/2- to 3 1/2-quart slow cooker with cooking spray. In slow cooker, mix all ingredients except crackers until well mixed."
        },
        {
            "@type": "HowToStep",
            "text": "Cover; cook on Low heat setting 2 hours. Serve dip warm with crackers."
        }
