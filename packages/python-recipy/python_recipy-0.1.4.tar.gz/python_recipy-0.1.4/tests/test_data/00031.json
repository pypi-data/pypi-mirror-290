    "recipeIngredient": [
        "5 tablespoons extra-virgin olive oil, divided",
        "2.5 teaspoons ground cumin, divided",
        "1.5 teaspoons dried marjoram",
        "0.75 teaspoon salt, divided",
        "0.25 teaspoon ground allspice",
        "0.25 teaspoon cayenne pepper",
        "1 pound boneless, skinless chicken breast, trimmed",
        "0.25 cup lemon juice",
        "2 cups fresh riced cauliflower (see Tip)",
        "2 cups flat-leaf parsley leaves",
        "1 cup diced cucumber",
        "1 cup halved cherry tomatoes",
        "0.25 cup sliced scallions"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat grill to medium-high."
        },
        {
            "@type": "HowToStep",
            "text": "Mix 2 tablespoons oil, 2 teaspoons cumin, marjoram, 1/2 teaspoon salt, allspice and cayenne in a small bowl. Brush on chicken."
        },
        {
            "@type": "HowToStep",
            "text": "Grill the chicken, turning occasionally, until an instant-read thermometer inserted in the thickest part registers 165 degrees F, 10 to 12 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Meanwhile, whisk lemon juice with the remaining 3 tablespoons oil, 1/2 teaspoon cumin and 1/4 teaspoon salt in a large bowl. Add riced cauliflower, parsley, cucumber, tomatoes and scallions; toss to coat."
        },
        {
            "@type": "HowToStep",
            "text": "Transfer the chicken to a clean cutting board and let rest for 5 minutes. Thinly slice the chicken and serve over the tabbouleh."
        }
    ],
    "recipeYield": [
        "4"
    ],
    "totalTime": "PT35M",
