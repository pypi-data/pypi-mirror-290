    "recipeIngredient": [
        "2 cups milk",
        "4 cups cubed or torn stale bread",
        "1/2 cup granulated sugar",
        "1/2 teaspoon salt",
        "1 teaspoon ground cinnamon or nutmeg",
        "2 eggs (lightly beaten)",
        "1/4 to 1/2 cup raisins"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/tb0RX0C89B8sfH9uBcE5R3o8_qA=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/old-fashioned-bread-pudding-with-raisins-3051799-step-01-98251b5694324855ae2628336636ef15.jpg"
                }
            ],
            "text": "Gather the ingredients. Preheat oven to 350 F."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/fQHbvh_v-U1T2n-PbYCCYFU70JU=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/old-fashioned-bread-pudding-with-raisins-3051799-step-02-02-f2077499dbb9449180eb7bc2bb610148.jpg"
                }
            ],
            "text": "Heat the milk just until scalded in a saucepan. Place bread cubes in a bowl; pour hot milk over bread. Cool."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/-RP0mg2Zg1NXN2bSm5tPZl3dc5I=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/old-fashioned-bread-pudding-with-raisins-3051799-step-03-fcb1ebaf302c4368a5eb4a24dbca1021.jpg"
                }
            ],
            "text": "Add the sugar, salt, nutmeg or cinnamon, eggs, and raisins."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/R7Hn_2liROp7J450Az5Z0jB0QOc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/old-fashioned-bread-pudding-with-raisins-3051799-step-04-e4cf93237de24f3c8ef6bfe38fbe261a.jpg"
                }
            ],
            "text": "Pour mixture into 8-inch square baking dish."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/TPXxB6LW6v1lyWwAdkm9V4TtBGI=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/old-fashioned-bread-pudding-with-raisins-3051799-step-05-2700470a309640fc81330f300c72b147.jpg"
                }
            ],
            "text": "Bake in a preheated oven for 20 to 25 minutes, or until set. Serve with a dessert sauce or whipped topping."
        }
    ],
