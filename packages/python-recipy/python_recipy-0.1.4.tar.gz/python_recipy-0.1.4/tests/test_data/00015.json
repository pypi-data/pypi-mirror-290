    "recipeIngredient": [
        "3 cups oats",
        "1.5 cups low-fat milk",
        "2 ripe bananas, mashed (about 3/4 cup)",
        "0.333 cup packed brown sugar",
        "2 large eggs, lightly beaten",
        "1 teaspoon baking powder",
        "1 teaspoon ground cinnamon",
        "1 teaspoon vanilla extract",
        "0.5 teaspoon salt",
        "1 cup fresh blueberries",
        "0.5 cup chopped toasted pecans"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 375°F. Coat a muffin tin with cooking spray."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.eatingwell.com/thmb/fmQUFZBOxn4WFlE5vd3as5_qCxM=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/baked-blueberry-and-banana-nut-oatmeal-cups-61849dacbf454142a51d046a4f6bd8e0.jpg"
                }
            ],
            "text": "Combine oats, milk, bananas, brown sugar, eggs, baking powder, cinnamon, vanilla and salt in a large bowl. Fold in blueberries and pecans. Divide the mixture between the muffin cups (about 1/3 cup each). Bake until a toothpick inserted into the center comes out clean, about 25 minutes. Cool in the pan for 10 minutes, then turn out onto a wire rack. Serve warm or at room temperature."
        }
    ],
