    "recipeIngredient": [
        "1 (7-ounce) container Greek yogurt",
        "2/3 c. thawed orange juice concentrate",
        "2 large bananas",
        "Zest of 1 lime",
        "1 tbsp. fresh lime juice"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Pur√©e all ingredients in a blender."
        },
        {
            "@type": "HowToStep",
            "text": "Pour into six 3-ounce molds. (We used mini baking tins with Popsicle sticks, but you can also substitute paper cups.) Freeze until smoothie pops are solid, about 4 hours."
        },
        {
            "@type": "HowToStep",
            "text": "To release pops, dip molds into hot water until pops loosen and slide out. If using cups, peel away paper."
        }
    ],
