    "recipeIngredient": [
        "3 tablespoons minced garlic",
        "2 tablespoons extra-virgin olive oil",
        "0.25 cup lemon juice",
        "0.25 cup minced fresh parsley",
        "0.5 teaspoon kosher salt",
        "0.5 teaspoon pepper",
        "1.25 pounds cooked shrimp"
