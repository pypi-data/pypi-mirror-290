    "recipeIngredient": [
        "1 large clove garlic, grated",
        "1 tablespoon red-wine vinegar",
        "1 tablespoon extra-virgin olive oil",
        "0.5 teaspoon lemon pepper",
        "0.5 teaspoon dried dill",
        "0.125 teaspoon salt",
        "16 large shrimp, shells and tails removed, deveined",
        "1 pound fat asparagus, woody ends snapped off, cut into 1 1/2-inch lengths"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Whisk garlic, vinegar, oil, lemon pepper, dill and salt in a small bowl. Divide the marinade between two large sealable plastic bags. Add shrimp to one and asparagus to the other. Agitate to combine, seal and refrigerate for 30 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Preheat grill to medium."
        },
        {
            "@type": "HowToStep",
            "text": "Thread the shrimp and asparagus alternately onto 8 skewers, dividing evenly among the skewers."
        },
        {
            "@type": "HowToStep",
            "text": "Oil the grill rack (see Tip) and immediately add the kebabs. Cook, turning every 2 to 3 minutes, until the shrimp are bright pink and just cooked through and the asparagus is tender, 5 to 6 minutes total."
        }
    ],
    "recipeYield": [
        "4"
    ],
