    "recipeIngredient": [
        "1/2 pound carrots, peeled and trimmed",
        "1/2 pound snow peas, trimmed",
        "1 tablespoon olive oil",
        "1/2 teaspoon finely chopped garlic",
        "1 tablespoon light soy sauce",
        "2 tablespoons chopped scallions",
        "Freshly ground pepper to taste"
    ],
    "recipeInstructions": [
        {
            "@context": "http://schema.org",
            "@type": "HowToStep",
            "text": "Slice the carrots into julienne strips about 1 1/2 inches long."
        },
        {
            "@context": "http://schema.org",
            "@type": "HowToStep",
            "text": "Place the carrots in a saucepan, cover with water and bring to a boil. Simmer 4 minutes. Add the snow peas, bring to a boil, stir and cook for 2 minutes. Drain."
        },
        {
            "@context": "http://schema.org",
            "@type": "HowToStep",
            "text": "Heat the oil in a skillet and add the carrots, snow peas, garlic, soy sauce, scallions and pepper. Stir and cook 1 minute. Serve."
        }
    ],
