    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "name": "Step 1",
            "text": "Place 1/4 cup apple cider vinegar, 1/4 cup honey, 3 tablespoons Dijon mustard,1/2 teaspoon kosher salt, and 1/4 teaspoon black pepper in a small bowl or small jar. Whisk or seal the jar and shake to combine.",
            "image": null
        },
        {
            "@type": "HowToStep",
            "name": "Step 2",
            "text": "Add 1/4 cup extra-virgin olive oil. Whisk vigorously or seal the jar and shake until combined and emulsified.",
            "image": null
        }
    ],
    "HowtoTip": {
        "text": "Storage: Refrigerate in an airtight container for up to 1 week. Rewhisk before using."
