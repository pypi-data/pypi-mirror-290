    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a blender, combine 3 scoops vanilla ice cream, 2 tablespoons milk, and 1 ounce Curaçao. Blend until creamy and pour into two glasses. Freeze 15 to 20 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Rinse blender and add 3 scoops vanilla ice cream, 2 tablespoons milk, and 1 ounce vodka. Blend until combined, then pour into glasses on top of blue layer and freeze 15 to 20 minutes. "
        },
        {
            "@type": "HowToStep",
            "text": "Rinse blender and add 3 scoops raspberry sorbet, 2 tablespoons milk, and 1 ounce raspberry vodka. Blend until combined, then pour onto white layer."
        },
        {
            "@type": "HowToStep",
            "text": "Top each shake with whipped cream and garnish with sprinkles."
        }
