    "name": "Matcha Whiskey Highball",
    "keywords": "matcha, whisky, highball",
    "recipeCategory": [],
    "recipeCuisine": [
        "Japanese Inspired",
        "Japanese"
    ],
    "recipeIngredient": [
        "2 ounces 12-year Japanese whisky, preferably Hibiki",
        "1/2 ounce fresh lemon juice",
        "1/2 ounce Honey Syrup",
        "1/4 teaspoon matcha green-tea powder",
        "4 ounces chilled club soda",
        "1 lemon wheel, for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Add whisky, lemon juice, honey syrup, and matcha into an ice-filled cocktail shaker. Shake vigorously. "
        },
        {
            "@type": "HowToStep",
            "text": "Pour into a chilled, ice-filled Collins glass. Pour the club soda into the shaker, swirl around to rinse, then stir into the glass. "
        },
        {
            "@type": "HowToStep",
            "text": "Garnish with the lemon wheel."
        }
    ],
