    "recipeIngredient": [
        "8 lb. butternut squash, peeled, seeded, and cut into 1-inch cubes",
        "2 large onions, chopped",
        "6 clove garlic",
        "3 tbsp. curry powder",
        "1 tsp. salt",
        "3/4 tsp. freshly ground pepper",
        "6 tbsp. vegetable oil",
        "8 c. low-sodium vegetable broth",
        "sour cream, for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 400Â°F. In a large bowl, toss butternut squash, onions, garlic, curry powder, salt, and pepper with vegetable oil to coat. Divide between 2 baking pans and roast until browned, 45 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Using a blender, puree batches of roasted vegetables with vegetable broth."
        },
        {
            "@type": "HowToStep",
            "text": "Transfer to a large pot and heat. Serve warm with a dollop of sour cream."
        }
    ],
