    "recipeIngredient": [
        "1 c. fresh lemon juice",
        "1/2 c. pure maple syrup",
        "1 1/2 strawberries, hulled and halved",
        "1 c. sparkling water",
        "1 c. blueberries (optional)"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine lemon juice, maple syrup, 3/4 pound strawberries, and 2 cups water in a blender. Blend until smooth, 1 to 2 minutes. Combine sparkling water, lemon juice mixture, remaining 3/4 pound strawberries, and, if desired, blueberries in a pitcher. Serve topped with ice."
        }
    ],
