    "recipeIngredient": [
        "3 tablespoons lemon juice",
        "2 tablespoons water",
        "2 tablespoons tahini",
        "1 small clove garlic, minced",
        "0.5 teaspoon salt",
        "0.125 teaspoon cayenne pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Whisk lemon juice, water, tahini, garlic, salt and cayenne in a small bowl until smooth."
        }
