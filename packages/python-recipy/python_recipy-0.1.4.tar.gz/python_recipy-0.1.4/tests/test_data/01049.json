    "recipeIngredient": [
        "3 cups all-purpose baking mix (such as Bisquick)",
        "1 lb. ground hot pork sausage, uncooked",
        "1 lb. sharp Cheddar cheese, shredded"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Prepare oven and baking sheets: Preheat the oven to 400°F. Lightly spray two baking sheets with cooking spray."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.southernliving.com/thmb/TTjX102wi08Ece0wEO9Co2AdHDY=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/Sausage_Balls_003-6239acb68e394149a2d6c5cba510edd8.jpg"
                }
            ],
            "text": "Combine ingredients: Combine all ingredients in a large bowl, pressing mixture together with hands."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.southernliving.com/thmb/ceARcoGExNr4-syI0PnWZg-SjpQ=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/Sausage_Balls_004-ed02cc9fb79a4605ac9ae7ebd1131ffd.jpg"
                }
            ],
            "text": "Make sausage balls: Shape into 3/4-inch balls, and place on lightly greased baking sheets."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.southernliving.com/thmb/ihUhfFlm3-PtTzbMnUZs6r8UGbI=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/Sausage_Balls_005-4e2bb205ca9e4d8397dc308c0bce19d3.jpg"
                }
            ],
            "text": "Bake sausage balls: Bake at 400ºF for 15 to 18 minutes or until lightly browned."
        }
    ],
