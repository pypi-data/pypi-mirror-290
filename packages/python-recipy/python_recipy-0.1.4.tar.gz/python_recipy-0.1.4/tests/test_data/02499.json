    "recipeIngredient": [
        "2 tablespoons fresh lime juice",
        "2 tablespoons rice vinegar",
        "1 tablespoon vegetable oil",
        "2 teaspoons sugar",
        "Salt",
        "0.5 small head shredded Savoy or green cabbage",
        "1 cup fresh cilantro leaves",
        "4 scallions, cut into matchsticks",
        "1 grated carrot",
        "0.5 fresh jalapeno, minced"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a large bowl, combine lime juice, rice vinegar, vegetable oil, and sugar; season with salt and whisk. To dressing, add cabbage, cilantro leaves, scallions, carrot, and jalapeno, and toss to combine."
        }
    ],
