    "recipeIngredient": [
        "1.5 cups crunchy peanut butter",
        "1 cup packed light-brown sugar",
        "0.5 cup (1 stick) softened unsalted butter",
        "1 large egg",
        "1.5 cups all-purpose flour, (spooned and leveled)",
        "1 teaspoon baking powder"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350 degrees. In a medium bowl, whisk together flour and baking powder; set aside."
        },
        {
            "@type": "HowToStep",
            "text": "In a large bowl, beat peanut butter, sugar, and butter until smooth. Beat in egg. Gradually add flour mixture, beating to combine."
        },
        {
            "@type": "HowToStep",
            "text": "Pinch off dough by the tablespoon; roll into balls. Place 2 inches apart on parchment-lined baking sheets. Using a fork, press balls in a crisscross pattern, flattening to a 1/2-inch thickness."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.marthastewart.com/thmb/_q2JPJx12WHSRtrq3buhVxiOO1A=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/peanut-butter-cookies-a100600_horiz-a80d93c3c2524851b11559d77dbe9109.jpgitok9ud94r9g"
                }
            ],
            "text": "Bake cookies, rotating halfway through, until lightly golden, 18 to 22 minutes. Cool cookies on a wire rack."
        }
    ],
