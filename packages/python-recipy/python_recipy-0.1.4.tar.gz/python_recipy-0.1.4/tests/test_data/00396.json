    "recipeIngredient": [
        "4 tablespoons butter",
        "0.5 cup all-purpose flour",
        "0.5 teaspoon salt",
        "0.125 teaspoon dry mustard",
        "0.125 teaspoon cayenne pepper",
        "2 cups milk",
        "1 teaspoon Worcestershire sauce",
        "2 cups shredded sharp Cheddar cheese"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a 2 quart saucepan over low heat, melt the butter. Stir in and blend the flour, salt, mustard and pepper. Stir in the milk and Worcestershire sauce. Continue stirring the mixture for 10 minutes, or until thickened."
        },
        {
            "@type": "HowToStep",
            "text": "Stirring continuously, melt the cheese into the mixture and blend well. Serve immediately over warm toast."
        }
    ],
