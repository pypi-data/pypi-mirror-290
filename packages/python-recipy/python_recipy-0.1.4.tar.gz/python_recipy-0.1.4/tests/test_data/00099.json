    "recipeIngredient": [
        "1 pound asparagus, trimmed",
        "1 tablespoon extra-virgin olive oil",
        "0.25 teaspoon salt",
        "0.25 teaspoon ground pepper",
        "0.5 cup fresh whole-wheat breadcrumbs",
        "2 tablespoons unsalted butter, at room temperature",
        "2 tablespoons minced chives",
        "3 anchovy fillets, minced",
        "2 cloves garlic, grated",
        "Zest and juice of 1 lemon"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Place a rimmed baking sheet in the oven; preheat to 450Â°F."
        },
        {
            "@type": "HowToStep",
            "text": "Toss asparagus with oil, salt and pepper in a medium bowl. Transfer to the hot pan. Roast until browned and tender-crisp, 4 to 6 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Meanwhile, toast breadcrumbs in a small dry skillet over medium-high heat, stirring frequently, until crispy, 3 to 5 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Combine butter, chives, anchovies, garlic and lemon zest in the bowl and mash into a paste with a fork."
        },
        {
            "@type": "HowToStep",
            "text": "Add the hot asparagus to the butter, drizzle with lemon juice and toss until the butter melts. Transfer to a serving platter and sprinkle with the breadcrumbs."
        }
    ],
