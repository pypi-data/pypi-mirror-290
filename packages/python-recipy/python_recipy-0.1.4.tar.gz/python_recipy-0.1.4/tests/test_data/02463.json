    "recipeIngredient": [
        "2 pounds shell-on large shrimp, shells split up the back and shrimp deveined",
        "2 teaspoons unsalted butter, melted",
        "2 teaspoons Old Bay seasoning"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Cook shrimp: In a large pot of boiling salted water, cook shrimp until opaque throughout, 4 to 5 minutes. Drain and transfer to a bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Toss with butter and seasoning: Add butter and Old Bay seasoning and toss to coat. Serve warm or at room temperature."
        }
    ],
