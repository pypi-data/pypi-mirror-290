    "recipeIngredient": [
        "1 cup confectioners' sugar, sifted",
        "1 (4 ounce) package cream cheese, softened",
        "1 teaspoon vanilla extract",
        "1 tablespoon milk, or as needed",
        "1.5 cups all-purpose flour",
        "2 tablespoons white sugar",
        "1 tablespoon unsweetened cocoa powder",
        "2 teaspoons baking powder",
        "1 teaspoon salt",
        "2 eggs",
        "1 cup buttermilk",
        "0.25 cup milk",
        "2 teaspoons red food coloring",
        "1 teaspoon distilled white vinegar",
        "0.25 cup melted butter",
        "cooking spray"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Beat together confectioners&#39; sugar, cream cheese, and 1 teaspoon vanilla in a large bowl with an electric mixer until smooth. Thin with 1 tablespoon milk to achieve a pourable consistency. Set glaze aside."
        },
        {
            "@type": "HowToStep",
            "text": "Whisk together flour, white sugar, cocoa powder, baking powder, and salt in a mixing bowl until evenly blended; set aside."
        },
        {
            "@type": "HowToStep",
            "text": "Beat eggs in a separate mixing bowl until smooth. Whisk in buttermilk, 1/4 cup milk, food coloring, vinegar, and remaining 1 teaspoon vanilla. Stir in flour mixture until moistened. Drizzle in melted butter; stir until incorporated and small lumps of flour remain."
        },
        {
            "@type": "HowToStep",
            "text": "Heat a lightly greased griddle over medium-high heat. Drop batter by large spoonfuls onto the hot griddle. Cook until bubbles form and the edges are dry. Flip and continue cooking until browned on the other side. Repeat with remaining batter."
        },
        {
            "@type": "HowToStep",
            "text": "Drizzle pancakes with glaze to serve."
        }
    ],
    "recipeYield": [
        "4"
    ],
