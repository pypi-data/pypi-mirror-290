    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Cook and drain fettuccine as directed on package."
        },
        {
            "@type": "HowToStep",
            "text": "Meanwhile, in 12-inch skillet, heat 1 tablespoon reserved artichoke marinade to boiling over medium-high heat. Cook onion in marinade about 2 minutes, stirring occasionally, until crisp-tender."
        },
        {
            "@type": "HowToStep",
            "text": "Stir in half-and-half; heat until hot. Stir in cheese, artichoke hearts and beef; heat until hot."
        },
        {
            "@type": "HowToStep",
            "text": "Add fettuccine; toss. Sprinkle with pepper."
        }
    ],
