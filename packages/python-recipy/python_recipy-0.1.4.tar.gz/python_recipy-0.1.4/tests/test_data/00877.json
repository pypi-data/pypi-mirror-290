    "recipeIngredient": [
        "1 ounce  vodka",
        "1 ounce coffee liqueur",
        "1 ounce  crème de cacao liqueur (dark)",
        "2 scoops  chocolate ice cream",
        "1 ounce chocolate syrup",
        "1 cup  crushed ice",
        "Garnish:  whipped cream",
        "Garnish:  maraschino cherry"
