    "recipeIngredient": [
        "4 medium-size ripe bananas",
        "1/2 cup butter",
        "1 cup packed light brown sugar",
        "Dash of ground cinnamon",
        "1/4 cup banana liqueur",
        "1/2 cup rum",
        "Vanilla ice cream"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Cut bananas in half crosswise, then lengthwise. Melt butter in a large skillet over medium heat. Add brown sugar, and cook, stirring constantly, 2 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Add bananas to skillet, and sprinkle with cinnamon. Remove skillet from heat. Stir in liqueur and rum, and carefully ignite the fumes just above mixture with a long match. Let flames die down."
        },
        {
            "@type": "HowToStep",
            "text": "Return to heat, and cook 3 to 4 minutes or until soft. Serve over ice cream."
        }
    ],
    "totalTime": "PT10M",
