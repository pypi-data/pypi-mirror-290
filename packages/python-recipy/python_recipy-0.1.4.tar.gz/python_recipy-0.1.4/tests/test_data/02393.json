    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In 12-inch skillet, mix milk, hot water, uncooked pasta and sauce mix (from Hamburger Helper box). Heat to boiling, stirring occasionally. Cut up 2 hot dogs; stir in."
        },
        {
            "@type": "HowToStep",
            "text": "Reduce heat; cover and simmer about 10 minutes, stirring occasionally, until pasta is tender. Remove from heat."
        },
        {
            "@type": "HowToStep",
            "text": "Cut lengthwise slit in each of remaining 6 hot dogs; place over macaroni mixture in skillet. Cover; let stand until hot. Serve in bowls; fill slit in each hot dog with customized toppings as desired."
        }
    ],
