    "recipeIngredient": [
        "1 pound fresh, ground pork sausage",
        "1 egg, beaten",
        "0.33300000429153 cup cracker meal",
        "0.5 teaspoon dried sage",
        "0.5 cup ketchup",
        "1 tablespoon white vinegar",
        "1.5 teaspoons brown sugar",
        "1 tablespoon soy sauce"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a medium-sized mixing bowl, combine sausage, egg, crushed crackers and sage. Separate the mixture into two balls."
        },
        {
            "@type": "HowToStep",
            "text": "Heat a skillet to a high heat and brown the sausage balls. Drain on paper towels. Transfer the balls to a container with a lid."
        },
        {
            "@type": "HowToStep",
            "text": "Combine ketchup, vinegar, brown sugar and soy sauce in a small pot. Stir while heating the sauce. Let the sauce come to a boil, then reduce the heat to simmer. Let the sauce simmer for 20 minutes before pouring the sauce over the balls."
        },
        {
            "@type": "HowToStep",
            "text": "Allow to cool, then cover the balls and sauce, and refrigerate overnight. Reheat before serving."
        }
    ],
