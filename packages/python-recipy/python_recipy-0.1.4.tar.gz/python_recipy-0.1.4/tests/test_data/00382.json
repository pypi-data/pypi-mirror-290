    "recipeIngredient": [
        "3 pork sausage patties",
        "1 large flour tortilla",
        "0.5 cup shredded asadero cheese, divided",
        "0.33333334326744 cup salsa",
        "1 medium egg",
        "4 slices avocado",
        "1 tablespoon chopped fresh cilantro, or to taste",
        "1 tablespoon crema con sal (salted Mexican-style sour cream)",
        "1 pinch freshly ground black pepper to taste"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "@type": "HowToStep",
            "text": "Crumble sausage into a large skillet and cook until browned over medium-high heat, about 5 minutes. Drain grease and remove browned pork to a plate. Clean out skillet."
        },
        {
            "@type": "HowToStep",
            "text": "Place flour tortilla into the skillet. Sprinkle 1/2 of the asadero cheese onto the tortilla and spread salsa over the cheese. Top with sausage crumbles and remaining cheese."
        },
        {
            "@type": "HowToStep",
            "text": "Crack open the egg and carefully add to the middle of the pizza; cook for 12 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Top pizza with avocado slices, chopped cilantro, crema con sal, and black pepper."
        }
    ],
