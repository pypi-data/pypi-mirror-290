    "recipeIngredient": [
        "3 red bell peppers",
        "1 medium yellow onion (peeled and diced)",
        "1/2 cup olive oil",
        "1/2 cup breadcrumbs",
        "3/4 cup walnuts (crushed)",
        "1 1/2 tablespoons chili powder or 1 fresh Fresno chile (chopped)",
        "1/4 teaspoon cumin"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 350 F."
        },
        {
            "@type": "HowToStep",
            "text": "Place the whole red peppers on a greased baking sheet."
        },
        {
            "@type": "HowToStep",
            "text": "Roast for about 10 to 12 minutes, turning approximately every four minutes. (If desired, you can also substitute jarred or deli ready-made roasted red peppers.)"
        },
        {
            "@type": "HowToStep",
            "text": "In the meantime, place the breadcrumbs in a food processor and add 2 tablespoons of cold water."
        },
        {
            "@type": "HowToStep",
            "text": "Blend to make a puree."
        },
        {
            "@type": "HowToStep",
            "text": "Add more water as needed."
        },
        {
            "@type": "HowToStep",
            "text": "Remove the peppers from the oven and peel off the skin."
        },
        {
            "@type": "HowToStep",
            "text": "Cut open and remove the seeds."
        },
        {
            "@type": "HowToStep",
            "text": "Cut into pieces and add to the breadcrumb mixture in the food processor."
        },
        {
            "@type": "HowToStep",
            "text": "In a small saute pan, saute the diced onions until lightly browned."
        },
        {
            "@type": "HowToStep",
            "text": "Add the sauteed onions and olive oil to the food processor and blend to a dip consistency."
        },
        {
            "@type": "HowToStep",
            "text": "Add the remaining ingredients and blend, adding olive oil as needed."
        },
        {
            "@type": "HowToStep",
            "text": "Remove from the food processor and place in a serving bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Serve with pita wedges, veggies, or use as a dipping sauce for kebabs ."
        }
    ],
