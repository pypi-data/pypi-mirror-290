    "recipeIngredient": [
        "Chili-lime seasoning (such as Tajín) for glass rim",
        "Fresh pineapple chunks (optional), plus more for garnish",
        "Jalapeno pepper slices (optional), plus more for garnish",
        "Ice",
        "3 Tbsp. (1.5 oz.) blanco tequila",
        "2 Tbsp. (1 oz.) pineapple juice",
        "1/2 cup (4 oz.) sparkling water (such as Topo Chico)",
        "1 Tbsp. (1/2 oz.) fresh lime juice "
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Put seasoning rim on glass: Use fresh pineapple or lime to wet the rim of the glass and then dip in a plate of chili-lime seasoning."
        },
        {
            "@type": "HowToStep",
            "text": "Muddle pineapple: Add pineapple chunks to glass and muddle with a spoon (this is where you would also add jalapeño slices for a spicy version). Add ice on top."
        },
        {
            "@type": "HowToStep",
            "text": "Mix ingredients: Add tequila, pineapple juice, and lime juice to glass. Top with Topo Chico. Garnish with pineapple chunks  and jalapeño slices, if using."
        }
    ],
