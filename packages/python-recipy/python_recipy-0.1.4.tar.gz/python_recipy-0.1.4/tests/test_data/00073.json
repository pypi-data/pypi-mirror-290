    "recipeIngredient": [
        "2 tablespoons extra-virgin olive oil",
        "1.75 pounds bone-in, skin-on chicken thighs, trimmed",
        "6 cloves garlic, minced",
        "2 teaspoons dried oregano",
        "0.75 cup dry white wine",
        "0.5 cup red-wine vinegar",
        "0.5 cup coarsely chopped pitted green olives",
        "0.25 cup chopped seeded lemon",
        "0.25 cup dried cherries",
        "0.25 cup packed light brown sugar",
        "0.5 teaspoon ground pepper",
        "0.25 teaspoon salt",
        "Chopped fresh parsley for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 400°F."
        },
        {
            "@type": "HowToStep",
            "text": "Heat oil in a large oven-safe skillet over medium-high heat. Pat chicken dry and add to the pan, skin-side down. Cook until lightly browned, 6 to 8 minutes. Flip the chicken and spoon out all but 2 tablespoons of the fat. Add garlic and oregano and cook until fragrant, about 1 minute. Add wine, vinegar, olives, lemon, cherries, brown sugar, pepper and salt; bring to a simmer."
        },
        {
            "@type": "HowToStep",
            "text": "Transfer the pan to the oven and roast until an instant-read thermometer inserted in the thickest part of a thigh without touching bone registers 165°F, about 20 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Transfer the chicken to a plate and return the pan to medium-high heat (remember, the handle will be hot). Cook until the sauce is slightly reduced, about 3 minutes. Serve the chicken with the sauce and sprinkle with parsley, if desired."
        }
    ],
