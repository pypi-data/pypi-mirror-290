                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/uF8vV8VpNGVqoRr8044iRghqpw8=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/SES-banana-smoothie-recipes-759606-step-04-cfcb332b146a489eb155f1bfd4a8acd9.jpg"
                }
