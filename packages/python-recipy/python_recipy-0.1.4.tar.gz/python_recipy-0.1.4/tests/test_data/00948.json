    "recipeIngredient": [
        "5 fresh blackberries",
        "Ice cubes",
        "2-1/2 ounces bourbon",
        "1 ounce sweet vermouth",
        "1 teaspoon simple syrup",
        "1/2 teaspoon aged balsamic vinegar",
        "2 dashes bitters",
        "Optional: Rosemary sprigs, fresh berries and orange wedges"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a shaker, muddle blackberries. Fill shaker three-fourths full with ice. Add the remaining ingredients; stir until beverage is well-chilled, 20-30 seconds. Strain into a coupe or cocktail glass, or an ice-filled rocks glass. Garnish as desired."
        }
    ],
