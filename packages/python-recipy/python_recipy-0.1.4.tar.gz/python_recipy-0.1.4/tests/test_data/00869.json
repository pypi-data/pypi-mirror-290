    "recipeIngredient": [
        "18 ounces semisweet chocolate (chopped)",
        "1-ounce unsweetened chocolate (chopped)",
        "6 ounces milk chocolate (chopped)",
        "1 (13-ounce) can evaporated milk",
        "1/4 cup powdered sugar",
        "1 teaspoon  vanilla",
        "Optional: fruit, cookies, and cake pieces (for dipping)"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "text": "Combine the semisweet chocolate, unsweetened chocolate, milk chocolate, evaporated milk, and powdered sugar in a 3-quart crock pot."
        },
        {
            "@type": "HowToStep",
            "text": "Cover the slow cooker and cook on LOW for 45 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Stir well, add the vanilla, stir again, and continue cooking, if necessary, on low for another 10 to 20 minutes to completely melt all of the chocolate."
        },
        {
            "@type": "HowToStep",
            "text": "This fondue can be held on low for another hour. Serve with dippers for a fabulous dessert. Think of unusual dippers to use— marshmallows , soft candy bars , and cookies would be delicious."
        },
        {
            "@type": "HowToStep",
            "text": "Enjoy!"
        }
    ],
