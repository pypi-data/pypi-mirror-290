    "recipeIngredient": [
        "2 tablespoons low-fat plain strained yogurt, such as Greek-style",
        "1.5 tablespoons crumbled cotija cheese",
        "1 tablespoon finely chopped fresh cilantro",
        "0.5 teaspoon lime zest",
        "0.125 teaspoon ground pepper",
        "1 multigrain sandwich thin, split and lightly toasted",
        "0.33 cup thinly sliced English cucumber",
        "0.25 cup arugula or mixed salad greens"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Stir yogurt, cotija, cilantro, lime zest and pepper together in a small bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Spread the mixture evenly on both halves of sandwich thin. Top one half with cucumber slices and arugula (or mixed salad greens). Place the other half on top. Cut the sandwich in half and serve."
        }
    ],
