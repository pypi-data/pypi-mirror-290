    "recipeIngredient": [
        "0.5 cup old-fashioned rolled oats (see Tip)",
        "0.5 cup unsweetened almond milk",
        "0.5 tablespoon chia seeds",
        "1 teaspoon maple syrup",
        "0.25 teaspoon ground cinnamon",
        "Pinch of salt",
        "0.5 cup diced apple",
        "2 tablespoons toasted pecans"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine oats, almond milk, chia seeds (if using), maple syrup, cinnamon and salt in a pint-sized jar and stir. Cover and refrigerate overnight."
        },
        {
            "@type": "HowToStep",
            "text": "Before serving, top with apple and pecans, if desired."
        }
    ],
