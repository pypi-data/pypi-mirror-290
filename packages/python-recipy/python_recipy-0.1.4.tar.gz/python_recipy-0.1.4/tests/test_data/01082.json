    "recipeIngredient": [
        "1 1/2 cups sour creamÂ ",
        "1 cup peeled, chopped English cucumber (from 1 medium [9 1/2 oz.] cucumber), plus more for garnish",
        "1/2 cup mayonnaise",
        "1/4 cup chopped red onion (from 1 small [5 oz.] onion), plus more for garnish",
        "2 tsp. ranch dressing mix (from 1 [1-oz.] envelope)",
        "1/4 tsp. kosher salt",
        "1/4 tsp. black pepper",
        "Chopped fresh flat-leaf parsley",
        "Wavy potato chips"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Stir together sour cream, cucumber, mayonnaise, red onion, ranch dressing mix, salt, and pepper in bowl until well combined. Cover, and refrigerate at least 30 minutes or up to 36 hours. Garnish with parsley, cucumber, and red onion; serve with chips."
        }
    ],
