    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Set the Instant Pot {6 Quart} to Saute setting."
        },
        {
            "@type": "HowToStep",
            "text": "Saute olive oil and onion until translucent about 1 to 2 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Add in rice and saute for 2 to 3 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Stir in chicken stock, salsa, and salt into the rice."
        },
        {
            "@type": "HowToStep",
            "text": "Lock the lid in place, set to manual high pressure and turn time down to 10 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Make sure the steam valve is set to sealed."
        },
        {
            "@type": "HowToStep",
            "text": "Once the time is up, push the steam valve open and let all the steam release."
        },
        {
            "@type": "HowToStep",
            "text": "Fluff the rice with a fork and serve."
        }
    ],
