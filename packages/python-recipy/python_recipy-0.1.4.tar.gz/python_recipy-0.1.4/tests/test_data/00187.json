    "recipeIngredient": [
        "1   tablespoon    olive oil",
        "1/4  cup    onion, diced ",
        "2   cups    long grain white rice",
        "2 1/3  cups    chicken stock",
        "1   cup    salsa",
        "1   teaspoon    salt"
    ],
