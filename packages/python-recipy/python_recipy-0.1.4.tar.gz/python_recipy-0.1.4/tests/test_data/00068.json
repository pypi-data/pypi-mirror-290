    "recipeIngredient": [
        "2 cups chopped fresh pineapple",
        "1 cup orange segments",
        "3 medium kiwis, peeled, halved and sliced (1/4-inch)",
        "1 medium crisp, sweet apple, such as Gala or Pink Lady, cubed",
        "1 cup seedless red grapes, halved",
        "2 tablespoons fresh lime juice"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine pineapple, oranges, kiwis, apple, grapes and lime juice in a large bowl. Let stand for 5 minutes, tossing occasionally, before serving."
        }
    ],
