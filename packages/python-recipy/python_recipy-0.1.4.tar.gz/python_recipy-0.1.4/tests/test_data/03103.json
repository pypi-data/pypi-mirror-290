    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/Kd5vesTusmr8S9VjEUpOWcq4mxs=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/ALR-244434-easy-two-ingredient-peanut-butter-fudge-VAT-step-01-ef3cf92438734019abb2ca1a8f6e915a.jpg"
                }
            ],
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/-krkOFofIut_RxlKUI6WR2ifprQ=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/ALR-244434-easy-two-ingredient-peanut-butter-fudge-VAT-step-02-3626bab68ab84880be7ec3cb39eb6fd5.jpg"
                }
            ],
            "text": "Line an 8-inch square pan with waxed paper; set aside."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/dV7ZNn0-SR3XpPd9NWnES8v3qGw=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/ALR-244434-easy-two-ingredient-peanut-butter-fudge-VAT-step-03-d476b1b9b3fe4bbead6878147bce44b6.jpg"
                }
            ],
            "text": "Spoon peanut butter and cream cheese frosting into a microwave-safe bowl; heat in the microwave on high for 30 seconds. Stir mixture and heat again for 30 seconds; stir again until thoroughly blended."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/vLtxrU-_sP5IavvrsCEuBzO13Wo=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/ALR-244434-easy-two-ingredient-peanut-butter-fudge-VAT-step-04-e9e96849951d438ab05a30cdfd7edb0c.jpg"
                }
            ],
            "text": "Pour mixture into the prepared baking pan. Refrigerate until chilled, about 1 hour before cutting into bite-sized squares."
        }
    ],
