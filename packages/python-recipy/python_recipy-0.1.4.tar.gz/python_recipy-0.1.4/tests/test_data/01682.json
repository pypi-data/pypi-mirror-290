    "recipeIngredient": [
        "2 ounces Cognac (such as Hennessy VS)",
        "1 teaspoon condensed coconut milk",
        "1/2 ounce sweet vermouth (such as Cocchi Torino)",
        "1/2 ounce pineapple liqueur ",
        "1/4 ounce cold brew ",
        "1 dash lime bitters ",
        "1 dash chocolate bitters ",
        "Orange twist (for garnish)"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Add Cognac and condensed coconut milk to a mixing glass and stir to dissolve, for about 20 seconds). "
        },
        {
            "@type": "HowToStep",
            "text": "Add sweet vermouth, pineapple liqueur, cold brew, lime bitters, and chocolate bitters to mixing glass. Stir to chill."
        },
        {
            "@type": "HowToStep",
            "text": "Fine strain drink into teacup, garnish with orange twist."
        }
    ],
