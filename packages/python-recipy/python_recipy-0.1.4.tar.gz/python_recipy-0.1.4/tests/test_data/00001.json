    "recipeIngredient": [
        "8 ounces whole-wheat fusilli",
        "2 tablespoons extra-virgin olive oil",
        "2 cups chopped onions",
        "2 cloves garlic, minced",
        "1 teaspoon dried thyme",
        "0.5 teaspoon salt",
        "0.25 teaspoon ground pepper",
        "1 cup low-sodium vegetable broth  or  chicken broth",
        "0.5 cup sour cream",
        "2 cups baby arugula, packed"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Bring a large saucepan of water to a boil. Add fusilli and cook according to package directions. Drain."
        },
        {
            "@type": "HowToStep",
            "text": "Meanwhile, heat oil in a large skillet over medium heat. Add onions and cook, stirring, for 5 minutes. Reduce heat to medium-low and cook, stirring, until golden, about 5 minutes more."
        },
        {
            "@type": "HowToStep",
            "text": "Add garlic, thyme, salt and pepper; cook, stirring, for 1 minute. Increase heat to medium-high and add broth; cook, stirring, until the liquid is reduced by half, about 2 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Reduce heat to medium and add sour cream, stirring until completely incorporated. Remove from heat and stir in the pasta and arugula."
        }
    ],
