    "recipeIngredient": [
        "One 6-pound duck, rinsed and cleaned",
        "Salt and pepper",
        "2 tablespoons minced garlic",
        "Duck fat"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 300 degrees F."
        },
        {
            "@type": "HowToStep",
            "text": "Rub salt, pepper and the minced garlic all over the duck and under the skin. Place the duck, breast down, in a deep braising pan and add duck fat to fully submerge; the amount of fat will vary depending on pan size."
        },
        {
            "@type": "HowToStep",
            "text": "Place the braising pan in the oven and cook for no less than 5 hours (see Cook's Note)."
        },
        {
            "@type": "HowToStep",
            "text": "Remove the duck from the fat and gently pull the meat off of the bones."
        }
    ],
