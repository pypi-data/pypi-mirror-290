    "totalTime": "PT35M",
    "recipeIngredient": [
        "1/2 c. red wine vinegar",
        "1/4 c. olive oil&nbsp;",
        "2 tbsp. Dijon mustard",
        "1 tsp. sugar",
        "1/4 red onion, thinly sliced",
        "1 lb. rotini pasta",
        "1 orange bell pepper, sliced",
        "1 pt. grape tomatoes, halved",
        "3/4 c. chopped marinated artichokes",
        "1/3 c. pitted kalamata olives, chopped",
        "4 oz. cured salami, such as Soppressata, chopped",
        "2 tbsp. chopped fresh flat-leaf parsley, plus more for serving",
        "1 tbsp. chopped fresh oregano",
        "2 tbsp. chopped capers",
        "14 oz. fresh mozzarella, torn"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Bring a large pot of salted water to a boil. Add pasta and cook according to package directions.&nbsp;"
        },
        {
            "@type": "HowToStep",
            "text": "Meanwhile, whisk together vinegar, oil, mustard, and sugar in a bowl. Transfer half of dressing to a large bowl and add onions; toss to coat. Let stand while pasta cooks. <br>"
        },
        {
            "@type": "HowToStep",
            "text": "Drain pasta and add to onion mixture; toss until liquid is absorbed. Cool to room temperature. <br>"
        },
        {
            "@type": "HowToStep",
            "text": "Add bell pepper, tomatoes, artichokes, olives, salami, parsley, oregano, capers, and remaining dressing, toss to coat.&nbsp; Fold in mozzarella. Serve sprinkled with additional parsley."
        }
    ],
    "recipeYield": "6 - 8 serving(s)",
