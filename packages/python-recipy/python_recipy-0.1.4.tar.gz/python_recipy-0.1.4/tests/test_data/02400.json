    "author": {
        "@type": "Person",
        "name": "Betty Crocker Kitchens"
    },
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In 4-quart Dutch oven, cook beef and bell pepper over medium-high heat 5 to 7 minutes, stirring occasionally, until beef is thoroughly cooked; drain."
        },
        {
            "@type": "HowToStep",
            "text": "Add tomatoes, tomato sauce and broth; heat to boiling. Stir in vegetables and salt. Reduce heat. Cover; simmer 30 minutes or until vegetables are tender."
        }
    ],
