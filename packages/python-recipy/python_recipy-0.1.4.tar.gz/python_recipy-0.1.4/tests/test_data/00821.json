    "recipeIngredient": [
        "1/3 cup pizza sauce",
        "3 tablespoons minced green onions",
        "1/4 cup pepperoni slices, cubed",
        "1/2 cup diced mozzarella cheese",
        "1/4 cup grated Parmesan cheese",
        "30 frozen mini filo shells"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350 F."
        },
        {
            "@type": "HowToStep",
            "text": "In a small bowl, combine the pizza sauce green onions, pepperoni, and mozzarella cheese."
        },
        {
            "@type": "HowToStep",
            "text": "Fill filo shells with this mixture."
        },
        {
            "@type": "HowToStep",
            "text": "Top the filo shells with the Parmesan cheese."
        },
        {
            "@type": "HowToStep",
            "text": "Bake the little filo shells for 15 to 20 minutes or until cheese is melted and cups are bubbly."
        },
        {
            "@type": "HowToStep",
            "text": "Let stand 5 minutes, then serve."
        }
    ],
    "recipeYield": [
        "8",
        "30 mini pizzas"
    ],
