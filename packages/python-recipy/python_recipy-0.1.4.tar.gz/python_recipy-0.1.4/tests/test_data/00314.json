    "recipeIngredient": [
        "Reynolds Wrap® Heavy Duty Aluminum Foil",
        "6 bone-in chicken pieces",
        "2 tablespoons olive oil, divided",
        "1.5 teaspoons dried basil",
        "1 teaspoon dried rosemary",
        "1 teaspoon garlic salt",
        "4 medium red-skin potatoes, quartered",
        "1 (8 ounce) package peeled baby carrots",
        "1 medium onion, cut in eighths"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 425 degrees F. Line a 13x9x2-inch pan with Reynolds Wrap® Heavy Duty Aluminum Foil."
        },
        {
            "@type": "HowToStep",
            "text": "Place chicken pieces in pan. Brush with 1 tablespoon olive oil."
        },
        {
            "@type": "HowToStep",
            "text": "Combine basil, rosemary and garlic salt in medium bowl; sprinkle 1/2 of the herb mixture over the chicken."
        },
        {
            "@type": "HowToStep",
            "text": "Add remaining olive oil, potatoes, carrots and onion to bowl with the herb mixture; stir to coat. Arrange vegetables in an even layer around chicken."
        },
        {
            "@type": "HowToStep",
            "text": "Cover dish with sheet of foil; fold back one corner of cover for heat circulation."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/hczVOLNv1LvX6g6opEJLdJvE2T4=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/Emilys-Herb-Roasted-Chicken-and-Vegetables-09083ebfdbca4d3cbd9eb4b876694d6c.jpg"
                }
            ],
            "text": "Bake 45 to 50 minutes or until vegetables are done and meat thermometer reads 170 degrees F for white meat and 180 degrees F for dark meat. Carefully remove cover."
        }
    ],
    "recipeYield": [
        "4"
    ],
