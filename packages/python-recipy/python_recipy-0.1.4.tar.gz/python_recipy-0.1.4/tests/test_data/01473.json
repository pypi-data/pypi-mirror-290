    "recipeIngredient": [
        "1 bunch kale, leaves stripped and blanched",
        "1 c. fresh basil leaves",
        "1/2 c. extra-virgin olive oil",
        "1/4 c. freshly grated Parmesan",
        "1/4 c. walnuts",
        "1 clove garlic, minced",
        "1 tsp. kosher salt"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In the bowl of a food processor, combine kale, basil, and oil and pulse until combined. Add Parmesan, walnuts, garlic, and salt and blend until combined."
        },
        {
            "@type": "HowToStep",
            "text": "Store in an airtight container up to 1 week."
        }
