    "recipeIngredient": [
        "3 cups powdered sugar",
        "3 Tbsp. light corn syrup",
        "1 tsp. vanilla extract (clear, if possible)",
        "1/4 tsp. kosher salt",
        "3-4 Tbsp. whole milk, divided"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine sugar, corn syrup, vanilla, salt, and milk: Stir together powdered sugar, corn syrup, vanilla, salt, and 3 tablespoons of the milk in a medium bowl until combined."
        },
        {
            "@type": "HowToStep",
            "text": "Add more milk if needed: Stir in up to remaining 1 tablespoon milk, 1/2 to 1 teaspoon at a time, until desired consistency is reached."
        }
    ],
