    "recipeIngredient": [
        "2 (14 ounce) cans low-sodium hearty-style tomato soup",
        "1 tablespoon olive oil",
        "3 cups chopped kale",
        "1 teaspoon minced garlic",
        "0.125 teaspoon crushed red pepper",
        "1 (14 ounce) can no-salt-added cannellini beans, rinsed",
        "0.25 cup grated Parmesan cheese"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat soup in a medium saucepan according to package directions; simmer over low heat as you prepare kale."
        },
        {
            "@type": "HowToStep",
            "text": "Heat oil in a large skillet over medium heat. Add kale and cook, stirring, until wilted, 1 to 2 minutes. Stir in garlic and crushed red pepper (if using) and cook for 30 seconds. Stir the greens and beans into the soup and simmer until the beans are heated through, 2 to 3 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Divide the soup among 4 bowls. Serve topped with Parmesan."
        }
    ],
