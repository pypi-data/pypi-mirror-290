    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Coarsely chop the parsley leaves and thin, tender stems. Finely chop the shallot and capers."
        },
        {
            "@type": "HowToStep",
            "text": "Combine the chopped parsley, shallots and capers and the lemon zest in a small bowl; mix well. Stir in the olive oil to reach the desired consistency (ideally just binding the other ingredients together). Season to taste with salt and pepper."
        }
