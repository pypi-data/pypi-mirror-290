                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/IfXCSYOnhaEEHXnBjMQJdCWMnT0=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/baked-tilapia-fillets-with-mustard-pecan-crust-3058106-step-08-b3c89c7b520f4a1aa508d6e254c4d25c.jpg"
