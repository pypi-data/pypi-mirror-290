    "recipeIngredient": [
        "45ml your choice of gin",
        "120ml Fever-Tree Mediterranean Tonic Water",
        "dehydrated lime, for garnish",
        "fresh bay leaf, for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "1.Fill a short tumbler glass with ice."
        },
        {
            "@type": "HowToStep",
            "text": "2.Add gin to glass, then pour over the Fever-Tree Mediterranean Tonic Water. Stir."
        },
        {
            "@type": "HowToStep",
            "text": "3.Clap the bay leaves in your hand to gently bruise as this releases its natural aroma and oil. Add to the drink as a garnish along with a slice of dehydrated lime."
        }
    ],
