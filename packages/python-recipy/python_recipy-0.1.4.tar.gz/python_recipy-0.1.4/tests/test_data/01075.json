    "name": "Hemingway Daiquiri",
    "keywords": "hemingway daiquiri, hemingway cocktail",
    "prepTime": "PT3M",
    "recipeCategory": [],
    "recipeCuisine": [
        "Southern"
    ],
    "recipeIngredient": [
        "2 oz. white rum",
        "0.75 oz. lime juice",
        "0.5 oz. grapefruit juice",
        "2 bar spoons (about 1/3 oz.) maraschino liqueur",
        "0.25 oz. aged rum (optional)",
        "Lime wheel or grapefruit wheel, for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Chill glass: Chill a coupe glass in the freezer for 10 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Shake daiquiri: Add white rum, lime juice, grapefruit juice, and maraschino liqueur to a cocktail shaker. Place the lid on the shaker, and dry shake (shake without ice) or 10 seconds. Open the shaker, and fill halfway with ice cubes. Place the lid back on the shaker, and shake vigorously for 30 seconds, or until the shaker becomes almost too cold to handle."
        },
        {
            "@type": "HowToStep",
            "text": "Pour daiquiri into coupe: Remove the coupe glass from the freezer, and strain the cocktail into the glass. Garnish with a lime slice or a grapefruit slice."
        }
    ],
