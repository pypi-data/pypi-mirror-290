    "recipeIngredient": [
        "250g udon noodle",
        "2 tsp vegetable oil",
        "100g peeled raw  prawn",
        "4 spring onions , chopped",
        "2 eggs , beaten",
        "2 tbsp roasted peanut , chopped",
        "small handful coriander leaves",
        "lime wedges, to serve",
        "2 tbsp tamarind paste",
        "1 tbsp fish sauce",
        "juice 1 lime",
        "1 tbsp soft  brown sugar"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Boil the noodles in salted water for 3 mins until soft. Drain and rinse in cold water. To make the sauce, mix all the ingredients together in a small bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Heat a wok with half the oil. Add the prawns and spring onions, and cook, stirring quickly, for 1 min or until the prawns turn pink. Push to the side and add the remaining oil. Add the egg and let sit for 30 secs, then scramble until cooked. Add the noodles and sauce, and cook, stirring continuously, for 3 mins or until everything is hot."
        },
        {
            "@type": "HowToStep",
            "text": "Serve the noodles with the peanuts and coriander sprinkled on top, and lime wedges for squeezing over."
        }
    ],
    "recipeYield": 2,
    "totalTime": "PT20M"
}