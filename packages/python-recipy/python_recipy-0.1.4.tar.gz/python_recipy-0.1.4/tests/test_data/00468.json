    "recipeIngredient": [
        "2 ounces Old Tom Gin",
        "3/4 ounce lemon juice",
        "1/2 ounce simple syrup",
        "2 ounces seltzer",
        "Lemon wheel and cherry flag, for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Add ingredients to shaker and shake cocktail: Add the gin, lemon juice, and simple syrup to a shaker half-filled with ice. Shake until it’s just chill to the touch, about 5 to 10 seconds."
        },
        {
            "@type": "HowToStep",
            "text": "Strain the cocktail: Strain the cocktail into the chilled Collins glass filled with ice."
        },
        {
            "@type": "HowToStep",
            "text": "Top with club soda and garnish: Top cocktail with seltzer. Garnish with a lemon wheel and cherry flag. Serve."
        }
    ],
