    "recipeIngredient": [
        "2.5 pounds baking potatoes",
        "1 teaspoon vegetable oil",
        "1 tablespoon white sugar",
        "1 teaspoon salt",
        "1 pinch ground cayenne pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 450 degrees F (230 degrees C). Line a baking sheet with foil, and coat well with vegetable cooking spray. Scrub potatoes well and cut into 1/2 inch thick fries."
        },
        {
            "@type": "HowToStep",
            "text": "In a large mixing bowl, toss potatoes with oil, sugar, salt and red pepper. Spread on baking sheet in one layer."
        },
        {
            "@type": "HowToStep",
            "text": "Bake for 30 minutes in the preheated oven, until potatoes are tender and browned. Serve immediately."
        }
    ],
    "recipeYield": [
        "6"
    ],
    "totalTime": "PT45M",
