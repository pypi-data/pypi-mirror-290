    "totalTime": "PT30M",
    "recipeIngredient": [
        "2 medium zucchini",
        "1 large egg",
        "2 oz. Pecorino cheese, grated&nbsp;",
        "1/2 c. panko&nbsp;",
        "1 clove garlic, pressed",
        "Black pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 400°F. Grate zucchini; squeeze dry with paper towels and transfer to a bowl.&nbsp;"
        },
        {
            "@type": "HowToStep",
            "text": "Add egg, Pecorino, panko, garlic, and pepper to zucchini; stir to combine. Drop heaping tablespoonfuls of mixture onto a greased baking sheet and gently flatten.&nbsp;"
        },
        {
            "@type": "HowToStep",
            "text": "Bake, flipping halfway through, until golden brown and crisp, 18 to 20 minutes."
        }
    ],
    "recipeYield": "4 serving(s)",
