    "recipeIngredient": [
        "2 teaspoons olive oil",
        "1 cup sliced carrots",
        "1 medium head green cabbage (about 1 1/2 pounds), sliced into 1/2-inch-wide shreds",
        "1 green bell pepper, cut into thin, bite-size strips",
        "2 tablespoons water",
        "0.5 teaspoon salt",
        "0.25 teaspoon ground pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat oil over medium heat in a large pot. Add carrots; cook for 5 minutes, stirring frequently."
        },
        {
            "@type": "HowToStep",
            "text": "Add cabbage, bell pepper, the water, salt, and pepper. Cover and cook for 10 to 15 minutes or until the cabbage and bell pepper are tender, stirring occasionally."
        }
    ],
