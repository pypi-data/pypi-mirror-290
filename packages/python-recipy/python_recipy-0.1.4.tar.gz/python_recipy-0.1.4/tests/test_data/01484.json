    "name": "Orange Creamsicle Dip",
    "prepTime": "PT5M",
    "cookTime": "PT0S",
    "totalTime": "PT5M",
    "recipeIngredient": [
        "1 8-oz. block cream cheese, softened",
        "8 oz. marshmallow crème",
        "1 can mandarin oranges, drained",
        "zest from 1 orange (optional)",
        "1 c. whipped topping, frozen (such as Cool Whip)",
        "1 package vanilla wafers, for dipping"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Using an electric mixer, beat cream cheese and <span id=\"docs-internal-guid-c4f98296-c8fb-f065-9c28-bda12dedbb13\">marshmallow crème</span> until thoroughly combined, about 1 minute."
        },
        {
            "@type": "HowToStep",
            "text": "Fold in mandarin oranges, zest, and whipped topping."
        },
        {
            "@type": "HowToStep",
            "text": "Serve chilled with vanilla wafers."
        }
    ],
