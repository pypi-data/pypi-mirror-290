    },
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In large skillet, cook ground beef and onion over medium-high heat until beef is thoroughly cooked, stirring frequently."
        },
        {
            "@type": "HowToStep",
            "text": "Stir in flour, bouillon and steak sauce. Gradually stir in milk. Cook and stir 4 to 6 minutes or until mixture boils and thickens, stirring frequently. If desired, serve over mashed potatoes or cooked rice."
        }
    ],
