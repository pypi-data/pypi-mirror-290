    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat gas or charcoal grill. In small bowl, mix oil, basil and garlic. Brush or drizzle oil mixture on cut sides of bread."
        },
        {
            "@type": "HowToStep",
            "text": "Carefully brush grill rack with vegetable oil. Place bread on grill over medium heat. Cook uncovered 4 to 6 minutes, turning once, until golden brown. Top with tomatoes."
        }
