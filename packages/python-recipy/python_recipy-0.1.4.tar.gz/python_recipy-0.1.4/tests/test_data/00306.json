    "recipeIngredient": [
        "2 zucchini, cut into 1/2-inch slices",
        "1 cup shredded Cheddar cheese",
        "2 tablespoons butter"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Prepare the broiler. Lightly grease a medium baking sheet."
        },
        {
            "@type": "HowToStep",
            "text": "Over medium-low heat melt butter in a large saucepan. Stir in zucchini and cook slowly, stirring occasionally until zucchini is soft and golden brown."
        },
        {
            "@type": "HowToStep",
            "text": "Spread zucchini on the baking sheet and top with cheese."
        },
        {
            "@type": "HowToStep",
            "text": "Broil until cheese is melted."
        }
    ],
    "recipeYield": [
        "8"
    ],
