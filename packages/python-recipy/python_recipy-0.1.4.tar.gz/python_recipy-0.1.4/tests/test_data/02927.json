    "recipeIngredient": [
        "150g manchego",
        "12 slices chorizo",
        "6 red grapes , halved",
        "12 mini mozzarella balls",
        "12 small  basil leaves (optional)",
        "6 slices prosciutto , halved"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat oven to 180C/160C fan/gas 4. Cut the manchego into 12 cubes or oblongs. Fold a slice of chorizo around each piece of cheese and push a cocktail stick through the middle to secure it. Push half a grape on to one side, then transfer skewers to a baking tray and cook for 5 mins. Cool."
        },
        {
            "@type": "HowToStep",
            "text": "Wrap each mozzarella ball in basil, top with half a slice of prosciutto and push a skewer through. To serve young children, slide everything off the skewers."
        }
    ],
    "recipeYield": 6,
    "totalTime": "PT25M"
