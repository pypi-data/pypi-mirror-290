    "name": "Caipirinha Manchada",
    "recipeCategory": [],
    "recipeCuisine": [
        "Mexican Inspired"
    ],
    "recipeIngredient": [
        "1/2 lime, cut into 6 pieces",
        "3/4 ounce Simple Syrup",
        "1 cup ice",
        "1 1/2 ounces cachaça (potent Brazilian sugarcane spirit)",
        "1/2 ounce Hibiscus Agua Fresca"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a cocktail shaker, muddle the lime pieces with the Simple Syrup. Add the ice and cachaça and shake well. Pour into a rocks glass and pour the Hibiscus Agua Fresca over a bar spoon on top."
        }
    ],
