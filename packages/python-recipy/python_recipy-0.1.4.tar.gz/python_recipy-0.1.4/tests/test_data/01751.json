    "recipeIngredient": [
        "5 pounds oxtails",
        "2 tablespoons vinegar",
        "5 cloves garlic",
        "3 stalks scallions",
        "3 sprigs fresh thyme",
        "1 small onion",
        "1 small fresh tomato",
        "1/2 bunch fresh cilantro",
        "Salt and ground pepper",
        "3 tablespoons vegetable oil",
        "3 tablespoons sugar",
        "Red beans and rice or white rice, for serving"
    ],
