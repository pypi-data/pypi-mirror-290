    "recipeIngredient": [
        "4 egg yolks",
        "1/3 cup sugar",
        "1 pint whole milk",
        "1 cup heavy cream",
        "1 1/4 ounces bourbon",
        "1 1/4 ounces dark rum",
        "1 teaspoon freshly grated nutmeg"
    ],
