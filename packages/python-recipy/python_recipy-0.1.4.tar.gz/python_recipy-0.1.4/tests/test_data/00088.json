    "recipeIngredient": [
        "0.75 cup unsalted cashews",
        "0.5 cup water",
        "0.25 cup packed parsley leaves",
        "1 tablespoon lemon juice or cider vinegar",
        "1 tablespoon extra-virgin olive oil",
        "0.5 teaspoon reduced-sodium tamari or soy sauce (see Tip)",
        "0.25 teaspoon salt",
        "0.5 cup cooked lentils",
        "0.5 cup cooked quinoa",
        "0.5 cup shredded red cabbage",
        "0.25 cup grated raw beet",
        "0.25 cup chopped bell pepper",
        "0.25 cup grated carrot",
        "0.25 cup sliced cucumber",
        "1 tablespoon Toasted chopped cashews for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Blend cashews, water, parsley, lemon juice (or vinegar), oil, tamari (or soy sauce) and salt in a blender until smooth."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.eatingwell.com/thmb/rMFFw_MoPNUei64pWSezBNFnf38=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/45654661-f67619abf7e141d09e587d2199a4f4da.jpg"
                }
            ],
            "text": "Place lentils and quinoa in the center of a shallow serving bowl. Top with cabbage, beet, pepper, carrot and cucumber. Spoon 2 tablespoons of the cashew sauce over the top (save extra sauce for another use). Garnish with cashews, if desired."
        }
    ],
