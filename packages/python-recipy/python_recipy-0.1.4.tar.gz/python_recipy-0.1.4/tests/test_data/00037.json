    "recipeIngredient": [
        "0.75 cup all-purpose flour",
        "0.75 cup whole-wheat flour",
        "2 teaspoons baking powder",
        "0.25 teaspoon salt",
        "2 large lemons",
        "0.75 cup 4%-fat cottage cheese",
        "2 large eggs",
        "0.75 cup granulated sugar plus 1 tablespoon, divided",
        "2 cups fresh blueberries",
        "6 tablespoons unsalted butter, melted, plus more for pan"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350Â°F. Coat a 9-by-5-inch loaf pan with butter; line with parchment paper, leaving a 1-inch overhang on all sides."
        },
        {
            "@type": "HowToStep",
            "text": "Whisk all-purpose flour, whole-wheat flour, baking powder and salt together in a large bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Grate both lemons to equal about 1/4 cup zest; place in a medium bowl. Juice 1 lemon to equal 2 tablespoons juice; add to the medium bowl. Slice the remaining lemon into thin rounds; remove seeds."
        },
        {
            "@type": "HowToStep",
            "text": "Add cottage cheese, eggs and 3/4 cup sugar to the bowl with the lemon zest and juice; whisk until combined."
        },
        {
            "@type": "HowToStep",
            "text": "Whisk the wet ingredients into the dry ingredients until just combined. Fold in blueberries and melted butter."
        },
        {
            "@type": "HowToStep",
            "text": "Transfer the batter to the prepared pan; top with the lemon slices and the remaining 1 tablespoon sugar."
        },
        {
            "@type": "HowToStep",
            "text": "Bake until the top is golden-brown and a toothpick inserted in the center comes out clean, 1 hour to 1 hour, 10 minutes. Let cool before slicing."
        }
    ],
