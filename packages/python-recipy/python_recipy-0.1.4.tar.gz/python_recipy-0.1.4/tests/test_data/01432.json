    "name": "Cherry Baked Brie",
    "prepTime": "PT15M",
    "cookTime": "PT0S",
    "totalTime": "PT30M",
    "recipeIngredient": [
        "1 lb. frozen cherries",
        "1/3 c. granulated sugar",
        "1/4 c. freshly squeezed lemon juice",
        "1/4 c. water",
        "1 tbsp. cornstarch",
        "1 7-oz. wheel brie",
        "Graham crackers, for serving"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350°. "
        },
        {
            "@type": "HowToStep",
            "text": "Make cherry compote: In a small saucepan over high heat, combine cherries with sugar, lemon juice, and water. Bring to a boil, stirring to dissolve sugar, then immediately reduce heat to low. Add cornstarch and simmer until cherries are tender and sauce is thickened, stirring occasionally, 7 to 8 minutes. Remove from heat. "
        },
        {
            "@type": "HowToStep",
            "text": "Place Brie into a baking dish and top with cherry compote. Bake until cheese is warm and melty, about 15 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Serve warm with graham crackers."
        }
    ],
