    "recipeIngredient": [
        "2 avocados",
        "1/3 c. shredded Cheddar",
        "8 slices bacon"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat broiler and line a small baking sheet with foil."
        },
        {
            "@type": "HowToStep",
            "text": "Slice each avocado in half and remove the pits. Peel the skin off of each avocado. "
        },
        {
            "@type": "HowToStep",
            "text": "Fill two of the halves with cheese, then replace with the other avocado halves. Wrap each avocado with 4 slices of bacon. "
        },
        {
            "@type": "HowToStep",
            "text": "Place bacon-wrapped avocados on the prepared baking sheet and broil until the bacon is crispy on top, about 5 minutes. Very carefully, flip the avocado using tongs and continue to cook until crispy all over, about 5 minutes per side. "
        },
        {
            "@type": "HowToStep",
            "text": "Cut in half crosswise and serve immediately. "
        }
    ],
