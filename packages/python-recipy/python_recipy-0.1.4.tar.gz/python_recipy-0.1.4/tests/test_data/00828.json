    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/ZP1hF3afwoQPojMK_YvAQ8BdqoY=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/microwave-corn-on-the-cob-5324592-step-01-0443e89ec2c743b4b225cd0a653f6737.jpg"
                }
            ],
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/6Iwaul6lonIPV5nmIH8evZHOo68=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/microwave-corn-on-the-cob-5324592-step-02-6d1a24b9f2be462daaf62bf5ae93358a.jpg"
                }
            ],
            "text": "Place the husk-on corn in the microwave. Cook on high for 4 minutes.Â "
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/sTc9BkxtDeg4S_PHFla2f4hrP58=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/microwave-corn-on-the-cob-5324592-step-03-1-61629e21745c400b84ad3d0230fdbae5.jpg"
                }
            ],
            "text": "Let cool for 30 seconds to a minute, then break or cut the bottom of the cob off. Pull back the husk and silk from corn. When cooked, the kernels will be plump, juicy and slightly darker in color."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/eqc-U-5IlHQDrD6bcjDFEhaDGuE=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/microwave-corn-on-the-cob-5324592-step-04-1-4b507c103cb34df89f9a8f404f40f00b.jpg"
                }
            ],
            "text": "Slather with softened butter, then season with salt and pepper. Garnish with fresh herbs. Serve immediately."
        }
    ],
    "recipeYield": [
