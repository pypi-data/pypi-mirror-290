    "recipeIngredient": [
        "1/2 cup sugar",
        "1/4 cup softened butter",
        "1/4 cup milk (I&#8217;ve also used greek yogurt)",
        "1 egg",
        "1 teaspoon vanilla",
        "1 teaspoon lemon zest (more or less to taste)",
        "1 cups flour",
        "1 1/2 teaspoons baking powder",
        "1/4 teaspoon salt",
        "1/2 cup white chocolate pieces",
        "3/4 cup fresh blueberries"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 350.",
            "url": "https://pinchofyum.com/white-chocolate-blueberry-muffins#instruction-step-1"
        },
        {
            "@type": "HowToStep",
            "text": "Beat the sugar, softened butter, milk, egg, lemon zest, and vanilla.",
            "url": "https://pinchofyum.com/white-chocolate-blueberry-muffins#instruction-step-2"
        },
        {
            "@type": "HowToStep",
            "text": "Sift together the flour, baking powder, and salt. Stir gently with previous mixture until combined. Batter should be pretty thick, but still soft and scoopable. If it&#8217;s too thick, add a little more milk.",
            "url": "https://pinchofyum.com/white-chocolate-blueberry-muffins#instruction-step-3"
        },
        {
            "@type": "HowToStep",
            "text": "Add white chocolate and blueberries and stir gently so you don&#8217;t break the blueberries.",
            "url": "https://pinchofyum.com/white-chocolate-blueberry-muffins#instruction-step-4"
        },
        {
            "@type": "HowToStep",
            "text": "Scoop into greased muffin tin and bake for 13-15 minutes.",
            "url": "https://pinchofyum.com/white-chocolate-blueberry-muffins#instruction-step-5"
        }
    ],
