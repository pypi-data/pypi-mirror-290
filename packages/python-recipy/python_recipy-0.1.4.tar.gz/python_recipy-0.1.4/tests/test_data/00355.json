    "recipeIngredient": [
        "1 serving cooking spray",
        "1 cup white sugar",
        "0.33333334326744 cup butter, softened",
        "2 eggs",
        "1.5 cups pawpaw pulp",
        "0.33333334326744 cup tepid water",
        "1.3333333730698 cups all-purpose flour",
        "0.5 cup chopped walnuts",
        "1 teaspoon baking soda",
        "0.5 teaspoon salt",
        "0.25 teaspoon baking powder"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350 degrees F (175 degrees C). Grease the bottom of an 8-inch pan with cooking spray."
        },
        {
            "@type": "HowToStep",
            "text": "Combine sugar and butter in a bowl; beat together until smooth. Beat in eggs. Add pawpaw pulp and water; beat until combined, about 30 seconds. Stir in flour, walnuts, baking soda, salt, and baking powder. Pour into the prepared loaf pan."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 1 hour 15 minutes. Allow to cool before slicing, about 30 minutes."
        }
    ],
