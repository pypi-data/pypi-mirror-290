    "recipeIngredient": [
        "1 1/2 cups water",
        "1/2 cup white wine",
        "1 lemon, juiced",
        "1 onion, chopped",
        "1/2 celery rib, chopped",
        "1 garlic clove, chopped finely",
        "1 teaspoon black peppercorns",
        "4 to 5 sprigs fresh thyme",
        "1 bay leaf"
