    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Place the beets in a medium pot and cover with water. Bring to a boil and cook until tender, about 40 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Drain and peel the skin while still warm, then cool in the fridge for about 1 hour."
        },
        {
            "@type": "HowToStep",
            "text": "Cut the beets into large cubes. Finely chop the mint leaves and add to the beets. Add the orange juice and some salt and pepper. Serve cold."
        }
