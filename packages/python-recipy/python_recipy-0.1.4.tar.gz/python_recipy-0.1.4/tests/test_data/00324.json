    "recipeIngredient": [
        "1 (16 ounce) package buttery round crackers",
        "1 pound sliced bacon, cut into thirds",
        "1 pound brown sugar"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 250 degrees F (120 degrees C)."
        },
        {
            "@type": "HowToStep",
            "text": "Arrange the crackers in a single layer on a large baking sheet. Top each cracker with 1/3 slice bacon, and sprinkle desired amount of brown sugar over all."
        },
        {
            "@type": "HowToStep",
            "text": "Bake 1 hour in the preheated oven, or until browned and crisp. Serve warm."
        }
    ],
