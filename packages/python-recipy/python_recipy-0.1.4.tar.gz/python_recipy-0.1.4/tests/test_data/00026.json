    "recipeIngredient": [
        "Nonstick cooking spray",
        "1.5 cups whole-wheat flour",
        "2 teaspoons ground cinnamon",
        "1 teaspoon baking powder",
        "1 teaspoon baking soda",
        "0.25 teaspoon ground nutmeg",
        "2 eggs, lightly beaten",
        "0.75 cup unsweetened applesauce",
        "0.25 cup agave syrup",
        "1.5 teaspoons vanilla",
        "1 cup fresh raspberries and/or halved strawberries",
        "3 ounces 70% to 80% dark chocolate, chopped",
        "0.33333334326744 cup chopped walnuts"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350 degrees F. Coat twelve 2 1/2-inch muffin cups with cooking spray. In a large bowl stir together the whole-wheat flour, cinnamon, baking powder, baking soda and nutmeg. Make a well in center of flour mixture."
        },
        {
            "@type": "HowToStep",
            "text": "In a small bowl combine eggs, applesauce, agave syrup and vanilla. Add egg mixture all at once to flour mixture. Stir just until moistened. Fold in berries, chocolate and walnuts. Spoon batter into the prepared muffin cups, filling each three-fourths full."
        },
        {
            "@type": "HowToStep",
            "text": "Bake 18 to 20 minutes or until a toothpick comes out clean. Cool in muffin cups on a wire rack."
        }
    ],
