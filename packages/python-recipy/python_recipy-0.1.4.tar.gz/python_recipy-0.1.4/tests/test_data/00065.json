    "recipeIngredient": [
        "0.5 ripe avocado",
        "0.5 cup buttermilk",
        "0.25 cup nonfat plain yogurt",
        "2 tablespoons chopped fresh chives",
        "2 tablespoons chopped fresh tarragon",
        "1 small clove garlic, grated",
        "2 teaspoons lemon juice",
        "1 teaspoon anchovy paste",
        "0.25 teaspoon salt",
        "0.25 teaspoon ground pepper",
        "2 bunches radishes (about 1 pound)"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine avocado, buttermilk, yogurt, chives, tarragon, garlic, lemon juice, anchovy paste, salt and pepper in a blender. Puree until smooth. Serve with radishes."
        }
    ],
