    "recipeIngredient": [
        "1 pound medium or large raw shrimp",
        "3/4 cup plain panko breadcrumbs",
        "4 green onions (thinly sliced)",
        "1 teaspoon Old Bay seasoning",
        "1/2 teaspoon freshly ground black pepper",
        "1/4 teaspoon garlic powder",
        "3 tablespoons mayonnaise",
        "2 teaspoons lemon juice",
        "1 large egg (beaten)",
        "3 tablespoons olive oil",
        "1 tablespoon butter",
        "For the Sriracha Mayonnaise:",
        "1/2 cup mayonnaise",
        "1 tablespoon Sriracha sauce"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToSection",
            "name": "Steps to Make It",
            "itemListElement": [
                {
                    "@type": "HowToStep",
                    "text": "Gather the ingredients."
                },
                {
                    "@type": "HowToStep",
                    "text": "Peel and devein the shrimp and then rinse under cold running water. "
                },
                {
                    "@type": "HowToStep",
                    "text": "Chop the shrimp finely and transfer them to a large bowl. Add the panko breadcrumbs, green onions, Old Bay, garlic powder and freshly ground black pepper. Toss to blend the ingredients thoroughly. Stir in the mayonnaise, lemon juice, and the beaten egg. Mix well. "
                },
                {
                    "@type": "HowToStep",
                    "text": "Line a pan or tray with wax paper or parchment paper. Shape the shrimp mixture into patties about 1/2-inch to 3/4-inch thick. Place the shaped shrimp cakes on the pan; cover and refrigerate for 30 minutes. "
                },
                {
                    "@type": "HowToStep",
                    "text": "Heat the warming drawer or oven to 200 F if you are cooking in advance or batches."
                },
                {
                    "@type": "HowToStep",
                    "text": "Lightly dust the shrimp cakes on both sides with the flour. Handle gently to keep them from falling apart."
                },
                {
                    "@type": "HowToStep",
                    "text": "Heat the olive oil and butter in a large skillet over medium heat. Cook the shrimp cakes in the hot fat in batches, for about 3 minutes per side. Let the patties brown thoroughly on one side before carefully turning to brown the other side."
                },
                {
                    "@type": "HowToStep",
                    "text": "Serve immediately with the Sriracha mayonnaise or transfer the cooked shrimp cakes to the warming drawer or oven to keep warm for up to 20 minutes."
                }
            ]
        },
        {
            "@type": "HowToSection",
            "name": "Sriracha Mayonnaise",
            "itemListElement": [
                {
                    "@type": "HowToStep",
                    "text": "Combine the 1/2 cup of mayonnaise with Sriracha sauce; blend thoroughly. Cover and refrigerate until serving time. "
                }
            ]
        }
    ],
