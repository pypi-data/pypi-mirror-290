    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a small mixing bowl whisk together the yogurt, mayonnaise, lemon juice, honey, vanilla extract and salt. Season with pepper, to taste. Set aside."
        },
        {
            "@type": "HowToStep",
            "text": "Place all of the fruit and the nuts into a large mixing bowl and toss to combine. Add the dressing, toss and serve."
        }
    ],
