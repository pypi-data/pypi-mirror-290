    "recipeIngredient": [
        "3/4 cup dry red wine",
        "1/3 cup orange juice",
        "4 garlic cloves, chopped",
        "4 shallots, chopped",
        "2 tablespoons rosemary, minced",
        "1 1/2 tablespoons oregano, minced",
        "3 tablespoons olive oil",
        "18 (2-to-3-ounce) lamb rib chops (or 12 loin chops)",
        "1 teaspoon salt",
        "1 teaspoon pepper",
        "6 teaspoons honey"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Whisk together dry red wine, orange juice, garlic, shallots, rosemary, oregano, and olive oil in a small bowl. Pour mixture into a large heavy-duty zip-top plastic bag, reserving 1/4 cup; add lamb chops. Seal bag, and chill 2 hours."
        },
        {
            "@type": "HowToStep",
            "text": "Remove chops from marinade, discarding marinade. Sprinkle with salt and pepper. Place chops on a lightly greased rack in a broiler pan."
        },
        {
            "@type": "HowToStep",
            "text": "Stir together reserved 1/4 cup wine mixture and honey."
        },
        {
            "@type": "HowToStep",
            "text": "Broil chops 3 inches from heat (with electric oven door partially open) 5 minutes on each side or to desired degree of doneness, basting with honey mixture after 3 minutes. If using loin chops instead of rib chops, broil loin chops 3 more minutes on each side."
        }
    ],
    "recipeYield": [
        "6"
    ],
