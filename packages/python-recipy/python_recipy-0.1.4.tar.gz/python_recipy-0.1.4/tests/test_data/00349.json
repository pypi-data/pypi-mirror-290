    "recipeIngredient": [
        "2 cups diced cooked chicken",
        "4 cooked bacon strips, crumbled",
        "0.5 cucumber, peeled and diced",
        "0.5 cup thinly sliced celery",
        "0.25 cup chopped Granny Smith apple",
        "0.75 cup olive oil mayonnaise",
        "0.25 cup reduced-fat crumbled blue cheese",
        "2 tablespoons finely chopped sweet onion (such as VidaliaÂ®)",
        "1 tablespoon dried basil",
        "1 teaspoon lemon juice",
        "0.5 teaspoon minced fresh ginger root",
        "1 dash Worcestershire sauce",
        "salt and ground black pepper to taste"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Mix chicken, bacon, cucumber, celery, and apple in a large bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Whisk mayonnaise, blue cheese, sweet onion, basil, lemon juice, ginger, Worcestershire sauce, salt, and pepper together in a bowl; pour over the chicken mixture and toss to coat."
        },
        {
            "@type": "HowToStep",
            "text": "Cover bowl with plastic wrap and refrigerate until chilled, at least 20 minutes."
        }
    ],
