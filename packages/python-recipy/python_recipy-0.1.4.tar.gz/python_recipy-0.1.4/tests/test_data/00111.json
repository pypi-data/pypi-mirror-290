    "recipeIngredient": [
        ".25 oz. Tanqueray London Dry (25 oz. per bottle)",
        ".25 oz. Smirnoff No. 21 Vodka (25 oz. per bottle)",
        ".25 oz. Captain Morgan Silver Spiced (25 oz. per bottle)",
        ".25 oz. Cuervo Especial Gold (25 oz. per bottle)",
        ".25 oz. triple sec",
        "1 oz. sweet and sour mix",
        "6 oz. cola",
        "1 lemon wedge"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Add Tanqueray London Dry, Smirnoff No.21 Vodka, Captain Morgan Silver Spiced, Cuervo Especial Gold, triple sec, and sweet and sour mix in ice-filled collins glass and stir."
        },
        {
            "@type": "HowToStep",
            "text": "Top with cola."
        },
        {
            "@type": "HowToStep",
            "text": "Garnish with lemon wedge (optional)."
        }
    ],
