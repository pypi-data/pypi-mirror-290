    "recipeIngredient": [
        "1.5 pounds ground turkey (93 percent lean)",
        "0.5 cup finely grated Gruyere cheese",
        "4 scallions, thinly sliced",
        "0.25 cup dried breadcrumbs",
        "0.25 cup Dijon mustard",
        "1 garlic clove, minced",
        "Coarse salt and ground pepper",
        "Oil"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat grill and form burgers: Heat grill to high. In a medium bowl, use a fork to gently combine ground turkey with Gruyere, scallions, breadcrumbs, mustard, and garlic; season generously with salt and pepper. Gently form mixture into four 1-inch-thick patties."
        },
        {
            "@type": "HowToStep",
            "text": "Oil grill and cook patties: Lightly oil grill. Place patties on hottest part of grill; sear until browned, 1 to 2 minutes per side."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.marthastewart.com/thmb/Nl5TzoHkaSq82r41dgI5Xcx-ieA=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/favorite-turkey-burger-a100107-horiz-0723-bbb84e2facc44d1e82ffc3cdaa8154c7.jpg"
                }
            ],
            "text": "Flip burgers and continue cooking Move patties to cooler part of grill; continue grilling until cooked through, 5 to 10 minutes per side."
        }
    ],
