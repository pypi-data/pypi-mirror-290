    "name": "Easy Burrito Bake",
    "keywords": "easy burrito bake",
    "recipeCategory": "Entree",
    "recipeCuisine": "Mexican",
    "image": "https://mojo.generalmills.com/api/public/content/TnKT2OwlKk60sO9-BwOGIQ_webp_base.webp?v=d5745153&t=85e6ab0127c04f66a40c0b82332e03a6",
    "description": "Add Old El Paso™ refried beans and salsa to make this delicious burrito topped with cheese, lettuce, tomatoes and olives – a delicious Mexican dinner.",
    "recipeIngredient": [
        "2  cups Original Bisquick™ mix",
        "1/2  cup cold water",
        "1  can (16 oz) Old El Paso™ Traditional Refried Beans",
        "2  cups cooked rice",
        "1/2  cup thick & chunky salsa",
        "2  cups shredded cheddar cheese (8 oz)",
        "2  cups shredded lettuce",
        "1  cup chopped tomato",
        "1/4  cup sliced ripe olives",
        "  Additional salsa, if desired",
        "  Sour cream, if desired"
    ],
    "totalTime": "PT0H50M",
    "prepTime": "PT0H15M",
    "recipeYield": "8",
    "aggregateRating": {
        "@type": "AggregateRating",
        "ratingValue": "3.5",
        "ratingCount": "20",
