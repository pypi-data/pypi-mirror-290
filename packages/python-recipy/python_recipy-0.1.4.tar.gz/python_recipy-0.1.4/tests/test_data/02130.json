    "recipeIngredient": [
        "2 cups (14 ounces) packed light brown sugar",
        "1/4 cup (1 3/4 ounces) granulated sugar",
        "1/2 teaspoon salt",
        "3 large eggs",
        "14 tablespoons (7 ounces) unsalted butter, melted",
        "3/4 cup sour cream",
        "2 teaspoons vanilla extract",
        "12 ounces chocolate stout (or regular stout)",
        "2 cups (10 ounces) all-purpose flour",
        "1 cup (3 ounces) cocoa",
        "1 3/4 teaspoons baking soda",
        "1/4 teaspoon baking powder"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Adjust oven rack to middle position and preheat oven to 350Â°F. Butter bundt pan and dust with cocoa."
        },
        {
            "@type": "HowToStep",
            "text": "In large bowl, whisk brown sugar, granulated sugar, salt, and eggs until combined. Whisk in butter, sour cream, vanilla, and stout until combined."
        },
        {
            "@type": "HowToStep",
            "text": "In medium bowl, whisk flour with cocoa, baking soda, and baking powder. Whisk into wet mixture until combined."
        },
        {
            "@type": "HowToStep",
            "text": "Pour into prepared pan and bake until just firm to touch and toothpick inserted into center comes out with moist crumbs, 45 to 50 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Transfer to wire rack to cool completely. Loosen edges with knife and invert onto serving plate."
        }
    ],
    "recipeYield": [
        "12",
        "1 bundt cake"
    ],
