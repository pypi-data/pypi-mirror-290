    "recipeIngredient": [
        "1.5 cups baby kale",
        "1 large apple, cored and roughly chopped",
        "2 tablespoons cashew butter or other nut butter",
        "2 teaspoons honey",
        "5-6 ice cubes",
        "1 cup unsweetened almond milk or other milk"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Add kale, apple, cashew butter (or other nut butter), honey, ice cubes and almond milk (or other milk) to a blender. Blend on medium-low speed, using the tamper as necessary, until well combined."
        },
        {
            "@type": "HowToStep",
            "text": "Increase speed to medium-high and blend until very smooth."
        }
    ],
