    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In deep fryer or 3-quart heavy saucepan, heat oil to 375°F."
        },
        {
            "@type": "HowToStep",
            "text": "In 1-gallon resealable food-storage plastic bag, mix flour, paprika, red pepper, and salt. Add chicken, seal bag and shake until chicken is coated with flour."
        },
        {
            "@type": "HowToStep",
            "text": "Fry chicken in batches (to avoid over crowding) about 10 minutes each or until brown and no longer pink in center. Drain on paper towels."
        },
        {
            "@type": "HowToStep",
            "text": "Meanwhile, in 1-quart saucepan combine butter, pepper sauce, black pepper and garlic powder; cook over medium-high heat, 3 to 5 minutes, stirring occasionally, until butter is melted and mixture is blended."
        },
        {
            "@type": "HowToStep",
            "text": "In large bowl, toss chicken with butter mixture until completely coated."
        }
    ],
