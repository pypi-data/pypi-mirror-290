    "prepTime": "PT20M",
    "recipeIngredient": [
        "1 tablespoon avocado oil",
        "2 tablespoons reduced-sodium tamari",
        "0.5 teaspoon smoked paprika",
        "8 ounces whole shiitake mushrooms, stems removed",
        "8 small slices whole-grain bread (or 4 large slices, halved)",
        "4 tablespoons vegan mayonnaise",
        "0.5 teaspoon finely grated or minced garlic",
        "1 avocado, halved and sliced",
        "8 thin slices tomato",
        "4 leaves romaine lettuce"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 375 degrees F."
        },
        {
            "@type": "HowToStep",
            "text": "Stir oil, tamari and paprika together in a medium bowl. Add shiitakes and stir until all the liquid is absorbed. Spread on a rimmed baking sheet and roast, turning once, until golden brown on both sides, about 30 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Toast bread. Stir mayonnaise and garlic together in a small bowl."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.eatingwell.com/thmb/TXMA5FCUDsZ9xZW3sPDvhDDkXs4=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/4784197-46f0eaf96d5a4cf295af7ff09ca4a245.jpg"
                }
            ],
            "text": "Spread 1 tablespoon of the mayonnaise mixture on each of 4 pieces of toast. Divide avocado, tomato, lettuce and the roasted shiitakes over the toasts. Top with the remaining toasts."
        }
    ],
