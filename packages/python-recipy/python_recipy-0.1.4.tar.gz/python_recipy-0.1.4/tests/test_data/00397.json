    "recipeIngredient": [
        "1 pound lean ground beef",
        "1 onion, chopped",
        "1 (28 ounce) can peeled and crushed tomatoes",
        "2 tablespoons vegetable oil",
        "salt to taste",
        "2 cups instant mashed potato flakes",
        "1 cup shredded Cheddar cheese"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat oil in frying pan. Add chopped onions, and fry until golden brown. Add ground beef, and keep stirring until meat is browned. Add crushed tomatoes, and continue to cook until liquid from the tomatoes evaporates. Salt to taste."
        },
        {
            "@type": "HowToStep",
            "text": "In the meantime, prepare the mashed potatoes as directed on the packet."
        },
        {
            "@type": "HowToStep",
            "text": "Spread the meat mixture into the bottom of an oven proof dish. Cover with a layer of mashed potatoes, and top with grated cheese."
        },
        {
            "@type": "HowToStep",
            "text": "Bake for 10 minutes at 400 degrees F (205 degrees C), or until cheese has melted and browned."
        }
    ],
