    "recipeIngredient": [
        "1 lb. refrigerated sugar cookie dough",
        "1 tbsp. ground cinnamon",
        "4 c. sliced peaches",
        "2 c. blueberries",
        "1/4 c. sugar",
        "1/4 c. all-purpose flour",
        "1 tsp. pure vanilla extract",
        "1/4 tsp. kosher salt",
        "Cooking spray, for pan",
        "2 tbsp. cinnamon sugar",
        "Vanilla ice cream, for serving"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 375°. In a large bowl, mix together cookie dough and cinnamon until combined. Set aside."
        },
        {
            "@type": "HowToStep",
            "text": "In another bowl, stir together peaches, blueberries, sugar, flour, vanilla and salt and stir together until combined. "
        },
        {
            "@type": "HowToStep",
            "text": "Spray a 9\"-x-13\" baking dish with cooking spray. Add fruit mixture to baking dish, then top with 1\" balls of sugar cookie dough and bake until fruit is bubbly and cookie crust golden, 1 hour. Sprinkle with cinnamon sugar."
        },
        {
            "@type": "HowToStep",
            "text": "Let cool, then serve with ice cream. "
        }
    ],
