    "recipeIngredient": [
        "2 cup ice cubes",
        "1 cup cored and halved fresh strawberries (from 8 oz. whole berries), plus whole berries for garnish",
        "1 cup chopped fresh pineapple (about 6 oz.), plus pineapple wedges for garnish",
        ".25 cup sweetened cream of coconut (such as Coco L칩pez) (from 1 [15-oz.] can)",
        ".25 cup fresh lime juice (from 2 limes)",
        ".25 cup (2 oz.) white rum (optional)"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Blend drink: Place ice cubes, halved strawberries, chopped pineapple, cream of coconut, lime juice, and rum (if using) in a blender. Process until smooth, 30 to 45 seconds."
        },
        {
            "@type": "HowToStep",
            "text": "Pour into glass and garnish: Pour evenly into 2 rocks or Collins glasses. Garnish each glass with a strawberry and a pineapple wedge. Serve immediately."
        }
    ],
