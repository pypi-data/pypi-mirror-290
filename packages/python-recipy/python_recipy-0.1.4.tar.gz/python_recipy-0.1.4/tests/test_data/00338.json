    "recipeIngredient": [
        "1 cup packed brown sugar",
        "0.5 cup white sugar",
        "0.5 teaspoon salt",
        "6 tablespoons cream sherry",
        "1 tablespoon grated orange zest",
        "1 teaspoon orange extract",
        "2 tablespoons butter",
        "5 cups walnut halves"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Line a baking sheet with aluminum foil or parchment paper."
        },
        {
            "@type": "HowToStep",
            "text": "Place the brown sugar, white sugar, salt, sherry, orange zest, and orange extract into a heavy saucepan, and stir until well combined. Bring to a boil over medium-low heat, stirring constantly; boil for 1 minute. Stir in the butter. Remove from heat, and stir in the walnut halves to coat with syrup."
        },
        {
            "@type": "HowToStep",
            "text": "Spread the candied walnut halves out onto the prepared baking sheet, and allow to cool thoroughly, at least 1 hour. Break into bite-size chunks to serve."
        }
    ],
