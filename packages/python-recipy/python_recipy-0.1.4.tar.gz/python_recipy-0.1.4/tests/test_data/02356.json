    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat oven to 325°F. Place cheese on cookie sheet. Bake 10 to 15 minutes or until softened."
        },
        {
            "@type": "HowToStep",
            "text": "Spoon cranberry sauce over cheese; sprinkle with almonds. Bake about 5 minutes longer or until sauce is heated. Serve with crackers."
        }
