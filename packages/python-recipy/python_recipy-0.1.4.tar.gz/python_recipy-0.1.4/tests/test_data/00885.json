    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "text": "In a cocktail shaker, muddle 3 slices of cucumber with the vodka, lime juice, simple syrup, and 3 dashes of hot sauce."
        },
        {
            "@type": "HowToStep",
            "text": "Add ice and shake well ."
        },
        {
            "@type": "HowToStep",
            "text": "Strain into an old-fashioned glass filled with fresh ice."
        },
        {
            "@type": "HowToStep",
            "text": "Top with club soda."
        },
        {
            "@type": "HowToStep",
            "text": "Garnish with a cucumber slice and add an extra dash of hot sauce. Serve and enjoy."
        }
