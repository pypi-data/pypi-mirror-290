    "recipeIngredient": [
        "1/4 cup garlic cloves, peeled",
        "1/4 onion, peeled",
        "1/4 cup chopped scallions",
        "1/4 cup ancho chile powder",
        "1 tablespoon freshly ground black pepper",
        "2 jalapeño peppers",
        "1/2 bunch fresh cilantro",
        "3/4 cup Budweiser or any other beer you got in the fridge",
        "Juice and grated zest of 1 orange",
        "Juice and grated zest of 2 limes",
        "1/2 kiwi fruit, peeled",
        "1/4 cup mirin",
        "Good pinch of kosher salt",
        "Good pinch of sugar",
        "1 pound skirt steak"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine all the ingredients for the marinade in a blender or food processor and purée."
        },
        {
            "@type": "HowToStep",
            "text": "Rub the marinade all over the steak and marinate the meat in the refrigerator, covered, for at least an hour and up to 2 days."
        },
        {
            "@type": "HowToStep",
            "text": "When you’re ready to grill, heat the grill to medium heat, brush with oil, and cook the steak for 10 minutes, until it’s nice and charred on the outside and medium on the inside."
        },
        {
            "@type": "HowToStep",
            "text": "Rest the meat for 5 to 10 minutes; then eat. Yum."
        }
    ],
