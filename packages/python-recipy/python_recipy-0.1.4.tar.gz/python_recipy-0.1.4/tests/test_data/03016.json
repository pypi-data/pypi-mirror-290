    "recipeIngredient": [
        "2 tablespoons extra-virgin olive oil",
        "8 boneless chicken thighs (3 pounds)",
        "Salt and freshly ground pepper",
        "1 medium onion (thinly sliced)",
        "1 red bell pepper (thinly sliced)",
        "2 pickled hot peppers (thinly sliced)",
        "3 garlic cloves (thinly sliced)",
        "0.5 cup dry red wine",
        "1.5 cups chicken stock",
        "1 pound ripe plum tomatoes (coarsely chopped)",
        "2 tablespoons chopped flat-leaf parsley"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a deep skillet, heat the olive oil. Season the chicken thighs with salt and pepper and add the chicken to the skillet, skin side down. Cook over moderately high heat, turning once until lightly browned and crisp, about 8 minutes. Transfer to a plate."
        },
        {
            "@type": "HowToStep",
            "text": "Add the onion, bell pepper, pickled peppers and garlic and cook over low heat, stirring occasionally, until softened, about 10 minutes. Pour in the wine and simmer, stirring occasionally, until nearly evaporated, about 5 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Add the chicken stock and tomatoes to the skillet and season lightly with salt and pepper. Return the chicken to the pan, nestling it in the vegetables skin side up, and bring to a simmer. Cover partially and cook over moderate heat for 30 minutes, until the chicken is tender and the sauce is reduced by half. Sprinkle with the parsley."
        }
    ],
