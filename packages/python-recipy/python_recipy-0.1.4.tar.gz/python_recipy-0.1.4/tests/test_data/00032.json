    "recipeIngredient": [
        "0.5 cup old-fashioned rolled oats (see Tip)",
        "0.5 cup water",
        "Pinch of salt",
        "3 tablespoons unsweetened coconut milk beverage",
        "2 tablespoons toasted unsweetened coconut chips",
        "2 teaspoons brown sugar",
        "0.25 teaspoon ground cinnamon",
        "0.125 teaspoon ground ginger",
        "Pinch of ground cardamom",
        "Pinch of ground pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine oats, water and salt in a jar or bowl and stir. Cover and refrigerate overnight."
        },
        {
            "@type": "HowToStep",
            "text": "In the morning, heat the oats, if desired, or eat cold. Top with coconut milk, coconut, brown sugar, cinnamon, ginger, cardamom and pepper."
        }
    ],
