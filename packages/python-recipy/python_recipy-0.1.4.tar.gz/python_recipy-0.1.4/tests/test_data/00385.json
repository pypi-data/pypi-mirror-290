    "recipeIngredient": [
        "0.5 cup evaporated milk",
        "1 cup light brown sugar, packed",
        "1 cup white sugar",
        "0.75 cup unsalted butter, room temperature",
        "0.25 teaspoon salt",
        "0.25 cup Irish cream liqueur",
        "1 teaspoon vanilla extract",
        "2.25 cups confectioners' sugar, sifted"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Grease an 8-inch square baking pan and line with 2 pieces of parchment paper about 14 inches long; allow the ends of the paper to hang over the sides of the pan by several inches."
        },
        {
            "@type": "HowToStep",
            "text": "Stir evaporated milk, light brown sugar, white sugar, unsalted butter, and salt in a heavy saucepan over medium heat until the mixture forms a thick, smooth syrup."
        },
        {
            "@type": "HowToStep",
            "text": "Bring to a boil, reduce heat to medium-low, and cook until a candy thermometer inserted into the center of the boiling syrup without touching the bottom of the pan reads 238 degrees F (112 degrees C), 20 to 30 minutes. Stir often. Candy temperature will rise slowly at first, then more quickly as temperature increases; watch carefully."
        },
        {
            "@type": "HowToStep",
            "text": "Remove saucepan from heat and stir in the Irish cream liqueur and vanilla extract until thoroughly combined."
        },
        {
            "@type": "HowToStep",
            "text": "Scrape the mixture into the work bowl of a large stand mixer fitted with beaters; set the machine on low speed and mix in confectioners&#39; sugar in 3 additions, allowing the sugar to blend in completely before adding the next addition. Scrape down the bowl frequently and mix fudge until completely smooth, about 2 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Scrape fudge into the prepared baking pan and place on a level surface to cool and set up, 3 to 5 hours."
        },
        {
            "@type": "HowToStep",
            "text": "Use the parchment paper ends as handles to lift the fudge out of the baking pan before slicing into cubes for serving."
        }
    ],
