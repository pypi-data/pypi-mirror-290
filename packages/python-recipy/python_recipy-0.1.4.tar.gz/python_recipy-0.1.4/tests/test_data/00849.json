    "recipeIngredient": [
        "For the Cake:",
        "2 cups (8 ounces) sifted cake flour",
        "2 1/2 teaspoons baking powder",
        "1/2 cup shortening",
        "1 1/4 cups granulated sugar",
        "2 large eggs, beaten",
        "3/4 teaspoon salt",
        "1 teaspoon vanilla extract",
        "2 ounces (2 squares) unsweetened baking chocolate, melted",
        "1 cup evaporated milk",
        "Chocolate Cream Cheese Frosting: ",
        "1/4 cup (2 ounces) unsalted butter, softened",
        "8 ounces cream cheese, softened",
        "2 ounces (2 squares) unsweetened chocolate, melted",
        "3 cups confectioners&#39; sugar",
        "5 to 6 tablespoons half-and-half, or light cream",
        "1 teaspoon vanilla extract"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToSection",
            "name": "Steps to Make It",
            "itemListElement": [
                {
                    "@type": "HowToStep",
                    "text": "Heat the oven to 350 F/180 C/Gas 4. Grease and flour two 9-inch layer cake pans."
                },
                {
                    "@type": "HowToStep",
                    "text": "Sift the sifted cake flour and baking powder together in a bowl; set aside."
                },
                {
                    "@type": "HowToStep",
                    "text": "In a large mixing bowl with an electric mixer, cream the shortening and granulated sugar until light and fluffy, about 4 to 5 minutes. Add the eggs, one at a time, beating well after each addition. Add salt and vanilla, then add the melted chocolate."
                },
                {
                    "@type": "HowToStep",
                    "text": "Add the flour mixture to the creamed mixture alternately with the evaporated milk, blending well after each addition and ending with flour mixture."
                },
                {
                    "@type": "HowToStep",
                    "text": "Divide the batter between the two prepared cake pans."
                },
                {
                    "@type": "HowToStep",
                    "text": "Bake in the preheated oven for about 30 to 35 minutes, or until cake springs back when lightly touched with finger."
                }
            ]
        },
        {
            "@type": "HowToSection",
            "name": "Chocolate Cream Cheese Frosting",
            "itemListElement": [
                {
                    "@type": "HowToStep",
                    "text": "In a mixing bowl with an electric mixer, cream together the butter and cream cheese."
                },
                {
                    "@type": "HowToStep",
                    "text": "Sift the confectioners&#39; sugar into a bowl."
                },
                {
                    "@type": "HowToStep",
                    "text": "Add the melted chocolate, salt, the sifted confectioners&#39; sugar, 5 tablespoons of half-and-half or light cream, and the vanilla."
                },
                {
                    "@type": "HowToStep",
                    "text": "Beat until smooth and spreadable. Add more confectioners&#39; sugar or cream, if necessary."
                }
            ]
        }
    ],
