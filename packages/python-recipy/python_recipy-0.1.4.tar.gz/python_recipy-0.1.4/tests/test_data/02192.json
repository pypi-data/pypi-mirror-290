    "recipeIngredient": [
        "1 cup flour",
        "1/2 teaspoon baking soda",
        "1/2 teaspoon salt",
        "8 tablespoons unsalted butter, at room temperature",
        "1/4 cup packed light brown sugar",
        "1/2 cup sugar",
        "1/2 teaspoon vanilla extract",
        "1 egg",
        "1/2 cup chocolate chips"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 375Â°F. Grease a 9 inch pie dish."
        },
        {
            "@type": "HowToStep",
            "text": "In a small bowl, whisk together flour, baking soda, and salt."
        },
        {
            "@type": "HowToStep",
            "text": "In a large bowl, beat together butter, brown sugar, and sugar until light and fluffy."
        },
        {
            "@type": "HowToStep",
            "text": "Add vanilla and eggs and beat until combined."
        },
        {
            "@type": "HowToStep",
            "text": "Add flour mixture to wet ingredients and beat until just combined. Stir in chocolate chips."
        },
        {
            "@type": "HowToStep",
            "text": "Press dough evenly across prepared pie dish. Bake until golden, about 20 minutes."
        }
    ],
    "recipeYield": "10",
