    "recipeIngredient": [
        "1 sweet pastry shell (unbaked and chilled)",
        "1 1/4 cups pastry cream (1/2 of creme patisserie recipe or most of frangipane recipe )",
        "3 or 4 large peaches (peeled and thinly sliced)",
        "3 tablespoons sugar",
        "1 tablespoon butter",
        "1/4 cup apricot jam",
        "1 tablespoon lemon juice (or water)"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 400 F (200 C)."
        },
        {
            "@type": "HowToStep",
            "text": "Spread the pastry cream (or frangipane) across the bottom of the chilled, unbaked sweet pastry shell."
        },
        {
            "@type": "HowToStep",
            "text": "Arrange the peach slices in an overlapping pattern over the pastry cream. Sprinkle the sugar over the peaches and dot the top of the fruit with tiny bits of butter."
        },
        {
            "@type": "HowToStep",
            "text": "Wrap strips of aluminum foil around the edges of the tart pan to protect the crust from burning."
        },
        {
            "@type": "HowToStep",
            "text": "Bake the tart in the preheated oven for about 40 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Remove the foil, and continue baking for another 10 minutes, or until the peaches and crust are nicely colored."
        },
        {
            "@type": "HowToStep",
            "text": "Remove the tart to a rack to cool."
        },
        {
            "@type": "HowToStep",
            "text": "While the tart is still warm, heat the apricot jam with the lemon juice until boiling. Use a pastry brush to apply the apricot glaze over the peaches."
        },
        {
            "@type": "HowToStep",
            "text": "Refrigerate the tart when it has cooled completely."
        },
        {
            "@type": "HowToStep",
            "text": "Serve chilled or at room temperature."
        }
    ],
