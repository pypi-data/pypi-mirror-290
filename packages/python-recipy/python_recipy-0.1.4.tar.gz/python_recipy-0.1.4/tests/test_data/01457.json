    "recipeIngredient": [
        "1 oz. vodka",
        "1 c. cooled hot chocolate",
        "5 maraschino cherries, plus more for serving",
        "2 tsp. Maraschino Cherry Juice",
        "Whipped cream, for serving",
        "Chocolate Sauce, for serving"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Add vodka, cooled hot chocolate, maraschino cherries, and cherry juice to a blender. Blend mixture then reheat hot chocolate."
        },
        {
            "@type": "HowToStep",
            "text": "Add heated hot chocolate to serving glass. Garnish with whipped cream, chocolate sauce, and a cherry."
        }
    ],
