        "bestRating": "5",
        "worstRating": "1"
    },
    "author": {
        "@type": "Person",
        "name": "Betty Crocker Kitchens"
    },
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In large microwavable bowl, mix cereals, pretzels, crackers and peanuts."
        },
        {
            "@type": "HowToStep",
            "text": "In medium bowl, mix remaining ingredients until well blended. Pour over cereal mixture, stirring until evenly coated. "
        },
        {
            "@type": "HowToStep",
            "text": "Microwave uncovered on High 7 minutes, stirring every 2 minutes. Spread on foil or paper towels to cool, about 15 minutes. Store in airtight container."
        }
    ],
