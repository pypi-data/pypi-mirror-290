    "recipeIngredient": [
        "1 gallon whole milk",
        "1 pound unsalted butter ",
        "2 cups all-purpose flour ",
        "1 teaspoon onion powder ",
        "21 ounces smoked gouda cheese, shredded",
        "Kosher salt and freshly ground black pepper",
        "3 1/2 pounds macaroni, cooked to al dente"
    ],
