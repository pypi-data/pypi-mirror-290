    "recipeIngredient": [
        "1 (9 inch) prepared shortbread pie crust",
        "2 (8 ounce) packages cream cheese",
        "1 cup white sugar",
        "2 large eggs",
        "1 cup sour cream",
        "2 teaspoons vanilla extract"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/gmJFPaNQQ0P5kTarHiNWoFQdIoQ=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/8419-easy-sour-cream-cheesecake-DDMFS-step-1-BG-2538-f1dcdf05638544069b9d12033944041f.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 325 degrees F (165 degrees C). Place pie crust in a pan."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/S_Xlyy39uOQInpZeMKXV3Q15big=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/8419-easy-sour-cream-cheesecake-DDMFS-step-2-BG-2542-7f21a1252ff447729b249b37226244e5.jpg"
                }
            ],
            "text": "Cream together cream cheese and sugar."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/pnh-ykIVpvddFIfIu7MMGQ-qPOg=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/8419-easy-sour-cream-cheesecake-DDMFS-step-3-BG-2550-2606df5678224f9cadd4ab8a66a493ad.jpg"
                }
            ],
            "text": "Add eggs one at a time, blending well."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/C2OkaPCqf2wHM8L-6Bv31AyGQuc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/8419-easy-sour-cream-cheesecake-DDMFS-step-4-BG-2553-1d08fc9da91546f1b5e2283b8f523693.jpg"
                }
            ],
            "text": "Add sour cream and vanilla."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/CPtUJ0yARX198ehMl8e4MzKGeus=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/8419-easy-sour-cream-cheesecake-DDMFS-step-5-BG-2559-c1139e98c1c545a1bf45d80cf336964d.jpg"
                }
            ],
            "text": "Pour into prepared crust in the pan."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in the preheated oven until the cake is set and jiggles evenly across the top when lightly shaken, 60 to 70 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/MYAhz7pn1EXt9yJYY52NdpkwPDM=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/8419-easy-sour-cream-cheesecake-DDMFS-step-6-BG-2589-db5165638536464489e15f17bbf50631.jpg"
                }
            ],
            "text": "Run a knife around the outside edge but leave the cake in the pan. Let cool on the counter, then place in refrigerator. Remove from pan when completely chilled, and serve."
        }
    ],
    "recipeYield": [
        "12"
    ],
