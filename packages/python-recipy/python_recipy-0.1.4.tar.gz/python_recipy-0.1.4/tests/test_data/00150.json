    "recipeIngredient": [
        "3 large egg whites",
        "1/2 cup sugar",
        "1/8 teaspoon salt",
        "1/4 teaspoon vanilla extract",
        "1 14-ounce package sweetened shredded coconut",
        "3 pints vanilla ice cream, slightly softened"
    ],
    "nutrition": {
        "@type": "NutritionInformation",
        "servingSize": "1 of 15 servings",
        "calories": "271",
        "fatContent": "15g",
