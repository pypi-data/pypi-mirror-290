    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/i_Ze3aC9InQbiaeYv5IKz9fy_Ys=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/easy-orange-sorbet-recipe-1945837-step-01-49e958831bc64b08965a5e268437ab74.jpg"
                }
            ],
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/F9fQbzFz8DX0tZcwaqVHAXkDVtc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/easy-orange-sorbet-recipe-1945837-step-02-5934784dd629400eb64c8a0746c9bf31.jpg"
                }
            ],
            "text": "Place the water and sugar in a small saucepan over medium-low heat."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/NiVoZAp1FY8eEiOcEWe_9qJcizU=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/easy-orange-sorbet-recipe-1945837-step-03-698667819f3349169ff2bc5546949d53.jpg"
                }
            ],
            "text": "Cook the syrup mixture, stirring occasionally until all the sugar is dissolved. This should take about 5 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/kShXk5E-qDGTo_pv5kXqtlX2hzc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/easy-orange-sorbet-recipe-1945837-step-04-fd9ffdd0d3d14f229ded108c0b3eb1b9.jpg"
                }
            ],
            "text": "Remove the sugar syrup from the heat and stir in the orange juice. Refrigerate the juice mixture until it is completely chilled, about 1 hour."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/LEwMe6r39Kjk04yxCClXP4KuBXc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/easy-orange-sorbet-recipe-1945837-step-05-431292b59652401e9693db17618e6a6a.jpg"
                }
            ],
            "text": "Stir in the Grand Marnier."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.thespruceeats.com/thmb/aEF12hMvwHxdv6sFR26E6gJVyFc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/easy-orange-sorbet-recipe-1945837-step-06-514113811b0e41c89e9bb0cd59b15e4d.jpg"
                }
            ],
