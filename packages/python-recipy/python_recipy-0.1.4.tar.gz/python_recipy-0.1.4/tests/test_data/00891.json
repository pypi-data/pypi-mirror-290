    "recipeIngredient": [
        "2 1/4 cups all-purpose flour",
        "2 1/2 teaspoons baking powder",
        "1/4 teaspoon baking soda",
        "1/2 teaspoon salt",
        "2 tablespoons granulated sugar",
        "5 tablespoons butter, melted",
        "6 ounces beer",
        "1/4 cup milk or cream, for brushing tops",
        "1 dash of paprika"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "text": "Heat oven to 375 F. Line a baking sheet with parchment paper or a silicone baking mat , or spray lightly with baking spray ."
        },
        {
            "@type": "HowToStep",
            "text": "Combine the flour, baking powder, baking soda, salt, and sugar in a mixing bowl. Whisk to blend thoroughly."
        },
        {
            "@type": "HowToStep",
            "text": "Add the melted butter and beer and stir until the mixture holds together."
        },
        {
            "@type": "HowToStep",
            "text": "Turn out onto a floured surface and knead 3 or 4 times, just enough to make a soft, cohesive dough."
        },
        {
            "@type": "HowToStep",
            "text": "Pat the soft dough into a circle about 3/4-inch thick."
        },
        {
            "@type": "HowToStep",
            "text": "Cut the biscuits out with biscuit cutters and place on the prepared baking sheet ."
        },
        {
            "@type": "HowToStep",
            "text": "Brush lightly with milk or cream and sprinkle with paprika."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in the preheated oven for about 15 minutes, or until the bottoms are browned"
        },
        {
            "@type": "HowToStep",
            "text": "Enjoy."
        }
    ],
