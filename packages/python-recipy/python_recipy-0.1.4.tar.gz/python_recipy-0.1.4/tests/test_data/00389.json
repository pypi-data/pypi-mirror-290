    "recipeIngredient": [
        "1.5 cups white sugar",
        "0.5 teaspoon ground nutmeg",
        "0.5 teaspoon ground cinnamon",
        "0.5 teaspoon ground allspice",
        "4 eggs, beaten",
        "0.25 cup butter, melted and cooled",
        "1.5 tablespoons cider vinegar",
        "0.5 teaspoon vanilla extract",
        "1 (9 inch) unbaked pie crust"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "@type": "HowToStep",
            "text": "In a mixing bowl, combine sugar, nutmeg, cinnamon and allspice. In a separate bowl, combine eggs, butter, vinegar and vanilla extract. Stir wet ingredients into the dry mixture. Pour entire mixture into the pie shell."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in a preheated 350 degrees F (175 degrees C) oven for 50 minutes or until firm. Cool on a rack."
        }
    ],
    "recipeYield": [
        "8"
    ],
