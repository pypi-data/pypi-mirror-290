    "recipeIngredient": [
        "18 jumbo pasta shells",
        "1 teaspoon extra-virgin olive oil",
        "2 cups diced zucchini (about 1 zucchini)",
        "1 cup chopped onion",
        "2 teaspoons minced garlic",
        "0.5 pound lean ground turkey",
        "1 cup part-skim ricotta cheese",
        "0.5 cup grated Parmesan cheese, divided",
        "1 large egg, lightly beaten",
        "2 teaspoons Italian seasoning",
        "1 teaspoon fennel seed",
        "0.25 teaspoon freshly grated nutmeg or 1/8 teaspoon ground nutmeg",
        "0.25 teaspoon crushed red pepper",
        "0.25 teaspoon salt",
        "0.25 teaspoon ground pepper",
        "2 cups no-sugar-added marinara sauce, divided",
        "2 tablespoons chopped fresh basil"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 375 degrees F."
        },
        {
            "@type": "HowToStep",
            "text": "Cook shells in a pot of boiling water according to package directions until al dente. Drain and transfer to a plate."
        },
        {
            "@type": "HowToStep",
            "text": "Heat oil in a large skillet over medium heat. Add zucchini, onion and garlic and cook, stirring often, until the vegetables are softened, about 5 minutes. Add turkey and cook, breaking it up with a wooden spoon, until cooked through, 3 to 4 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Whisk ricotta, 1/4 cup Parmesan, egg, Italian seasoning, fennel seed, nutmeg, crushed red pepper, salt and pepper in a large bowl. Add the turkey mixture and mix well."
        },
        {
            "@type": "HowToStep",
            "text": "Spread 1 cup marinara in the bottom of a 9-by-13-inch baking dish. Spoon 2 heaping tablespoons of the ricotta mixture into each shell; arrange in the baking dish, seam-side up. Top the filled shells with the remaining 1 cup sauce and sprinkle with the remaining 1/4 cup Parmesan."
        },
        {
            "@type": "HowToStep",
            "text": "Cover the baking dish with foil and bake until the marinara is bubbly, about 30 minutes. Uncover and bake for 10 minutes more. Let the shells rest for 10 minutes, then sprinkle with basil, if desired, and serve."
        }
    ],
    "recipeYield": [
