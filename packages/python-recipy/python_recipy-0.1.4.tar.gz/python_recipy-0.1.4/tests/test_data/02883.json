    "recipeIngredient": [
        "Kosher salt and freshly ground pepper",
        "2 1/4 lb. fresh green beans",
        "4 slices bacon, chopped",
        "2 medium shallots, chopped",
        "1/4 c. chopped pecans (about 1 ounce)",
        "6 tbsp. white-wine vinegar",
        "1 1/2 tbsp. sugar"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a large pot over high heat, bring 2 1/2 quarts salted water to a boil. Working in batches, add green beans and cook 2 minutes per batch. Transfer to a bowl. Set aside."
        },
        {
            "@type": "HowToStep",
            "text": "In a large pan over medium heat, cook bacon until crisp, about 7 minutes. Transfer to a paper-towel-lined plate; set aside. SautÃ© shallots for 2 minutes; add pecans and cook for 1 minute. Remove pan from heat; add vinegar and sugar, stirring until sugar dissolves."
        },
        {
            "@type": "HowToStep",
            "text": "Add reserved green beans to pan; cook over low heat until heated through, about 4 minutes. Season with salt and pepper. Transfer to a dish; top with reserved bacon."
        }
    ],
