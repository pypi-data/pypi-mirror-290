    "recipeIngredient": [
        "2 tablespoons beef drippings ",
        "6 ounces all-purpose flour ",
        "6 ounces milk (whole or semi-skimmed)",
        "4 ounces water",
        "2 free-range eggs ",
        "Salt"
    ],
