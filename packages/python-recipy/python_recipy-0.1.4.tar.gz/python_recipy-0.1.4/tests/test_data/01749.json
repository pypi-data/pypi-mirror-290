    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 425 degrees F."
        },
        {
            "@type": "HowToStep",
            "text": "Pour the beef drippings into a muffin pan. Place the pan in the oven until the drippings are smoking hot. "
        },
        {
            "@type": "HowToStep",
            "text": "Meanwhile, mix together the flour, milk, water, eggs and some salt and pepper in a bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Divide the batter among the muffin cups and return the pan to the oven. "
        },
        {
            "@type": "HowToStep",
            "text": "Bake until golden brown and crisp, 25 to 35 minutes, depending on the oven. "
        }
    ],
