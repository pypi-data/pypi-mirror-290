    "recipeIngredient": [
        "1 c. all-purpose flour, sifted",
        "1/4 c. cornstarch",
        "1/2 tsp. kosher salt",
        "12 egg whites, at room temperature",
        "1 tsp. cream of tartar",
        "1 1/2 c. granulated sugar",
        "1 tsp. pure vanilla extract",
        "1/2 tsp. almond extract",
        "Whipped cream, for serving",
        "Sliced strawberries, for serving",
        "Blueberries, for serving"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350°. In a large bowl, sift&nbsp;together flour, cornstarch, and salt.&nbsp;"
        },
        {
            "@type": "HowToStep",
            "text": "In another large bowl beat egg whites and cream of tartar until soft peaks form, then gradually add sugar until stiff peaks form.&nbsp;"
        }
    ],
