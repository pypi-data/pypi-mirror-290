    "headline": "Port o' Call Cocktail",
    "name": "Port o' Call Cocktail",
    "recipeIngredient": [
        "1 ounce Guinness",
        "1 1/4 ounce Martell VSOP (or other nice, assertive cognac)",
        "1/2 ounce St-Germain",
        "1/4 ounce crème de cacao",
        "3 dashes Angostura bitters",
        "Orange zest, for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Add all of the ingredients to a mixing glass with ice. Stir and then strain into a chilled coupe. Garnish with orange zest."
        }
    ],
