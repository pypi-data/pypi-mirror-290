    "recipeIngredient": [
        "1 pound broccoli florets",
        "3 teaspoons olive oil",
        "1 tablespoon minced garlic",
        "2 teaspoons za'atar, divided, or more to taste",
        "0.25 teaspoon salt, divided",
        "0.25 teaspoon ground black pepper, divided",
        "1.5 pounds skin-on salmon fillets",
        "1 medium lemon, thinly sliced",
        "0.25 cup crumbled feta cheese"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 425 degrees F (220 degrees C). Line a 10x15-inch baking pan with aluminum foil."
        },
        {
            "@type": "HowToStep",
            "text": "Arrange broccoli in a single layer on the prepared pan. Drizzle with 2 teaspoons oil. Sprinkle with garlic, 1/2 of the za&#39;atar, and 1/8 teaspoon each salt and pepper."
        },
        {
            "@type": "HowToStep",
            "text": "Roast in the preheated oven for 10 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Drizzle salmon with remaining 1 teaspoon oil, remaining za&#39;atar, and remaining salt and pepper. Top with lemon slices. Add to the pan. Roast until vegetables are tender and fish flakes easily when tested with a fork, 12 to 15 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Sprinkle with feta cheese."
        }
    ],
