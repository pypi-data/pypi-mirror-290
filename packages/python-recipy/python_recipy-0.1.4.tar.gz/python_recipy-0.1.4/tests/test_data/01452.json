    "recipeIngredient": [
        "1 lb. salmon",
        "Kosher salt",
        "Freshly ground black pepper",
        "1 tbsp. Cajun spice",
        "2 tbsp. extra-virgin olive oil",
        "1 c. diced pineapple",
        "2 ripe avocados, diced",
        "1/4 c. red onion, diced",
        "2 small tomatoes, diced",
        "Juice of 1 lime, plus wedges for serving",
        "Freshly chopped cilantro, for garnish",
        "6 corn tortillas, warmed or grilled"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Pat salmon dry, if necessary, then evenly season all sides of the filets with salt, pepper, and Cajun spice."
        },
        {
            "@type": "HowToStep",
            "text": "In a large skillet over medium heat, heat oil. When oil is hot but not smoking, add salmon. Cook until deeply golden, about 5 to&nbsp;6 minutes, then flip and cook for another 2 to&nbsp;3 minutes, until salmon is opaque."
        },
        {
            "@type": "HowToStep",
            "text": "Let rest and cool slightly, then flake into large pieces."
        },
        {
            "@type": "HowToStep",
            "text": "Make pineapple avocado salsa: In a medium bowl, add pineapple, avocado, onion, tomatoes, and lime juice. Season with salt and toss to combine."
        },
        {
            "@type": "HowToStep",
            "text": "Assemble tacos: Top tortillas with salmon, avocado salsa, and cilantro. Serve with lime wedges for squeezing."
        }
    ],
