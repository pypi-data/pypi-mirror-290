    "recipeIngredient": [
        "For the Crust:",
        "30 single graham crackers (about 1 cups of crumbs)",
        "1/4 cup granulated sugar",
        "6 tablespoons unsalted butter (melted)",
        "For the Lime Filling:",
        "3 egg yolks, (at room temperature)",
        "1 (14-ounce) can sweetened condensed milk",
        "1/2 cup lime juice",
        "1/4 teaspoon salt",
        "1 lime (zested)",
        "Garnish: lime slices"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "text": "Pre-heat the oven to 350 F."
        },
        {
            "@type": "HowToStep",
            "text": "Add the graham crackers to a food processor and pulse until you have crumbs. Add in the sugar and melted butter."
        },
        {
            "@type": "HowToStep",
            "text": "Press the graham cracker mixture into the bottom of an 8&#34; x 8&#34; square baking pan. Note that a glass pan works very well for this recipe."
        },
        {
            "@type": "HowToStep",
            "text": "Bake for 10 minutes and allow to cool slightly, while leaving the oven on."
        },
        {
            "@type": "HowToStep",
            "text": "For the lime filling: Add the egg yolks to a mixing bowl and, using a stand or hand mixer, beat for a few minutes until fluffy."
        },
        {
            "@type": "HowToStep",
            "text": "Mix in the sweetened condensed milk, lime juice, salt, and lime zest."
        },
        {
            "@type": "HowToStep",
            "text": "Pour the mixture into the graham cracker crust and return to the oven for 10 to 12 minutes, or until set."
        },
        {
            "@type": "HowToStep",
            "text": "Cool to room temperature and then refrigerate for 4 hours."
        },
        {
            "@type": "HowToStep",
            "text": "Slice into bars and serve with a wedge of lime on top, if desired. "
        }
    ],
