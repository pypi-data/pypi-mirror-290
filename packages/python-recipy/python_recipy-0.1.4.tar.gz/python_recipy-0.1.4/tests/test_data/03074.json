    "recipeIngredient": [
        "2 cups all-purpose flour",
        "1 teaspoon baking soda",
        "1 teaspoon ground cinnamon",
        "0.5 teaspoon ground cloves",
        "0.5 teaspoon salt",
        "1 cup white sugar",
        "0.5 cup margarine, softened",
        "1 large egg",
        "1 cup grated zucchini",
        "1 cup raisins"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Whisk together flour, baking soda, cinnamon, cloves, and salt in a medium bowl."
        },
        {
            "@type": "HowToStep",
            "text": "Beat sugar and butter together in a large bowl with an electric mixer until smooth and creamy, 2 to 3 minutes. Beat in egg, then stir in zucchini. Gradually stir in flour mixture until incorporated. Mix in raisins. Cover dough and refrigerate for at least 1 hour or overnight."
        },
        {
            "@type": "HowToStep",
            "text": "When ready to bake, preheat the oven to 375 degrees F (190 degrees C). Grease two cookie sheets."
        },
        {
            "@type": "HowToStep",
            "text": "Drop teaspoonfuls of chilled dough 2 inches apart onto the prepared cookie sheets."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in the preheated oven until set, 8 to 10 minutes, switching racks halfway through."
        },
        {
            "@type": "HowToStep",
            "text": "Remove from the oven. Allow cookies to cool slightly on the cookie sheets before transferring to wire racks to cool completely."
        }
    ],
