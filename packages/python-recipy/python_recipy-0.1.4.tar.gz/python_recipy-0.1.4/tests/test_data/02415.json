    "recipeIngredient": [
        "1 pound (4 sticks) unsalted butter, room temperature, plus more for baking sheets",
        "3 cups packed light-brown sugar",
        "1 cup granulated sugar",
        "4 large eggs",
        "2 teaspoons pure vanilla extract",
        "3.5 cups all-purpose flour",
        "1.5 teaspoons salt",
        "2 teaspoons baking soda",
        "1.5 cups best-quality chocolate chips"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine wet ingredients: Cream butter until smooth; add sugars, and beat until smooth. Beat in eggs and vanilla."
        },
        {
            "@type": "HowToStep",
            "text": "Sift together dry ingredients: Into a large bowl, sift together dry ingredients."
        },
        {
            "@type": "HowToStep",
            "text": "Add dry ingredients to wet; stir in chips: Slowly beat dry ingredients into wet mixture. Fold in chocolate chips."
        },
        {
            "@type": "HowToStep",
            "text": "Scoop dough onto baking sheets; refrigerate: Use 1/4-cup scoop to scoop dough onto ungreased baking sheets; space at least 2 inches apart to allow for spreading. Cover with plastic wrap and refrigerate for at least an hour or up to 2 days."
        },
        {
            "@type": "HowToStep",
            "text": "Preheat oven; bake cookies Preheat oven to 375Â°F. Remove cookies from refrigerator and bake until golden, 10 to 12 minutes; rotating the pan halfway through. Remove cookies from baking sheets, and allow to cool on baking racks."
        }
    ],
