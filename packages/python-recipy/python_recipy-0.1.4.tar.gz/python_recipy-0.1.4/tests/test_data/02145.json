    "recipeIngredient": [
        "ice",
        "1 ounce gin (Tanqueray or Beefeater recommended)",
        "1 ounce fresh lime juice",
        "1 ounce Cherry Heering or Bols Cherry Brandy",
        "1 ounce Benedictine (optional)",
        "chilled club soda",
        "3 dashes Angostura bitters",
        "optional: pineapple spear, cherry, and/or orange slice for garnish"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine gin, lime juice, Cherry Heering, and Benedictine in a Collins glass filled with ice. Top with club soda, and dash bitters into the drink. Garnish with a pineapple spear, cherry, orange slice or nothing at all."
        }
    ],
