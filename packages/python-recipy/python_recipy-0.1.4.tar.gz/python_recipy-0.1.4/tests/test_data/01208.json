    "recipeIngredient": [
        "1 banana, fresh or frozen",
        "1 cup mango chunks, fresh or frozen",
        "1/2 cup wild blueberries, fresh or frozen",
        "1/2 cup plain nonfat Greek yogurt",
        "1/2 cup milk (or just enough to help things blend smoothly)",
        "1 heaping tablespoon raw honey (more to taste)",
        "1/2 cup kale or any other add ins that you want"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Place bananas, mangoes, blueberries, yogurt, and milk in a blender. Blend until smooth. Add the kale and honey; blend again until smooth.",
            "url": "https://pinchofyum.com/honey-wild-blueberry-smoothie#instruction-step-1"
        },
        {
            "@type": "HowToStep",
            "text": "If desired (and especially if all your ingredients were fresh instead of frozen), add a few ice cubes to increase the volume of the smoothie and help it stay chilled. Crush until smooth.",
            "url": "https://pinchofyum.com/honey-wild-blueberry-smoothie#instruction-step-2"
        },
        {
            "@type": "HowToStep",
            "text": "Top with additional blueberries and serve immediately.",
            "url": "https://pinchofyum.com/honey-wild-blueberry-smoothie#instruction-step-3"
        }
    ],
