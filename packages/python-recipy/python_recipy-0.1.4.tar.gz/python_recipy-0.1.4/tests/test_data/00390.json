    "recipeIngredient": [
        "1 (6 ounce) jar pitted green olives, drained",
        "1 (5 ounce) jar pitted Kalamata olives, drained",
        "1 (2 ounce) jar capers, drained",
        "0.5 cup olive oil",
        "0.5 cup flat-leaf parsley",
        "3 cloves garlic, or more to taste",
        "1 tablespoon lemon juice",
        "0.5 teaspoon salt",
        "0.5 teaspoon dried oregano",
        "freshly ground black pepper to taste"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine green olives, Kalamata olives, capers, olive oil, parsley, garlic, lemon juice, salt, oregano, and pepper in a food processor. Pulse 5 to 10 times until all ingredients are chopped and well combined."
        }
    ],
