    "recipeIngredient": [
        "Cooking spray",
        "1 whole chicken, cut into pieces",
        "1/2 tsp. garlic powder ",
        "1/2 tsp. salt ",
        "1/2 tsp. ground black pepper",
        "1 jar (12-oz.) apricot preserves",
        "1 bottle (8-10 oz.) Russian salad dressing (such as Wishbone)",
        "1 medium onion, finely chopped (about 1 cup)"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 350°F. Grease a 13- x 9-in. pan with cooking spray."
        },
        {
            "@type": "HowToStep",
            "text": "Pat the chicken pieces dry with a paper towel and rub them with garlic powder, salt, and pepper. Place in prepared pan."
        },
        {
            "@type": "HowToStep",
            "text": "Whisk together the apricot preserves and the Russian dressing. Stir in the chopped onion. Pour over the chicken evenly."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.southernliving.com/thmb/RwPBxiW8cqc8_mXcr8Z55jb32A4=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/Southern-Living-Apricot_Chicken_001-0615712d9bc344bbbb3160719b8e50a2.jpg"
                }
            ],
            "text": "Bake 1 hour, or until a thermometer inserted in thickest portion of chicken registers 165°F. Tent the dish with aluminum foil if the skin is getting too dark during baking."
        }
    ],
    "recipeYield": [
        "6"
