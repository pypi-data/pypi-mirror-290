    "recipeIngredient": [
        "1 package cheese ravioli (fresh or frozen)",
        "1/4 c. olive oil",
        "2 cloves garlic, chopped",
        "1/2 c. walnuts, chopped",
        "8 oz. asparagus, chopped",
        "1/4 c. grated Parmesan (1 oz)"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Cook the ravioli according to package directions."
        },
        {
            "@type": "HowToStep",
            "text": "Meanwhile, heat the oil in a medium skillet over medium heat. Add the garlic and walnuts and cook, stirring often, until the nuts are lightly toasted and fragrant and the garlic is golden brown, 4 to 5 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Add the asparagus and cook, tossing occasionally, until just tender, about 2 minutes. Serve over the ravioli and sprinkle with the Parmesan."
        }
    ],
