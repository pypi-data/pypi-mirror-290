    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Melt butter in a saucepan over medium heat. Add sugar, corn syrup, vanilla, salt and 1/3 cup water. Whisk until well combined and melted. Just as it begins to bubble, slowly add the heavy cream, whisking while incorporating into the caramel."
        },
        {
            "@type": "HowToStep",
            "text": "As soon as the caramel bubbles, remove from heat and let cool to room temperature."
        },
        {
            "@type": "HowToStep",
            "text": "If the caramel appears to separate, simply whisk together. Serve warm. Enjoy!"
        }
    ],
