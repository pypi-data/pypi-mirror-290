    "recipeIngredient": [
        "3/4 cup (1 1/2 sticks) unsalted butter",
        "3 cups light brown sugar ",
        "1/3 cup corn syrup ",
        "1 tablespoon vanilla bean paste ",
        "1 teaspoon salt ",
        "1/3 cup heavy cream "
    ],
