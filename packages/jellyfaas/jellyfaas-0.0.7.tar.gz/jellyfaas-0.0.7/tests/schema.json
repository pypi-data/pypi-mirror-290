{
  "tests": [
    {
      "name": "test1",
      "schema": {
        "requirements": {
          "requestType": "POST",
          "queryParams": [
            {"name": "param1", "required": true, "description": "The first parameter.", "exampleData": "id=12345ABC"},
            {"name": "param2", "required": false, "description": "The second parameter (optional).", "exampleData": "cell=Sheet2!B3"}
          ],
          "inputSchema": {
            "type": "object",
            "required": true,
            "properties": {
              "data": {"type": "string", "required": true},
              "details": {
                "type": "object",
                "required": true,
                "properties": {
                  "createdAt": {"type": "string", "required": true}
                }
              }
            }
          },
          "inputDescription": "Input data and details",
          "outputSchema": {
            "type": "object",
            "required": true,
            "properties": {
              "id": {"type": "string", "required": true},
              "result": {"type": "string", "required": true}
            }
          },
          "outputDescription": "Response with ID and result"
        }
      },
      "cases": [
        {
          "input": {
            "data": "some data",
            "details": {"createdAt": "2024-01-01T00:00:00Z"}
          },
          "expected_result": "valid"
        },
        {
          "input": {
            "data": 12345,
            "details": {"createdAt": 12345}
          },
          "expected_result": "invalid"
        }
      ]
    },
    {
