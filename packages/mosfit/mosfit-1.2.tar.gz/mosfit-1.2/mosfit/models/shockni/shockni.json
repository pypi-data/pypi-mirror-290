{
    "@references": [
        {
            "name": "Moore et al., in preparation"
        }
    ],
    "ebv": {
        "kind": "parameter",
        "value": 0.0,
        "class": "parameter",
        "inputs": [
            "transient"
        ],
        "latex": "E({\\rm B} - {\\rm V})"
    },
    "fnickel": {
        "kind": "parameter",
        "value": 1.0,
        "class": "parameter",
        "latex": "f_{\\rm Ni}"
    },
    "rvhost": {
        "kind": "parameter",
        "value": 3.1,
        "class": "parameter",
        "latex": "R_{\\rm V, host}"
    },
    "nhhost": {
        "kind": "parameter",
        "value": 1.0e16,
        "class": "parameter",
        "latex": "n_{\\rm H,host}",
        "derived_keys": "avhost"
    },
    "texplosion": {
        "kind": "parameter",
        "value": -0.47406,
        "class": "parameter",
        "latex": "t_{\\rm exp}\\,{\\rm (days)}",
        "requests": {
            "min_times": "transient"
        }
    },
    "temperature": {
        "kind": "parameter",
        "value": 2500,
        "class": "parameter",
        "latex": "T_{\\rm rec}\\,{\\rm (K)}"
    },
    "kappa": {
        "kind": "parameter",
        "value": 0.2,
        "class": "parameter",
        "latex": "\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma": {
        "kind": "parameter",
        "value": 10.0,
        "class": "parameter",
        "latex": "\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta": {
        "kind": "parameter",
        "value": 1.0,
        "class": "parameter",
        "latex": "M_{\\rm ej}\\,(M_\\odot)"
    },
    "vejecta": {
        "kind": "parameter",
        "value": 1.0e4,
        "class": "parameter",
        "latex": "v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "shock_frac": {
        "kind": "parameter",
        "value": 1,
        "class": "parameter",
        "latex": "\\zeta_{\\rm shock}"
    },
    "radius_star": {
        "kind": "parameter",
        "value": 1e12,
        "class": "parameter",
        "latex": "R_{\\rm star}\\,({\\rm cm})"
    },
    "s": {
        "kind": "parameter",
        "value": 1,
        "class": "parameter",
        "latex": "s"
    },
    "shock_cooling": {
        "kind": "engine",
        "inputs": [
            "densetimes",
            "s",
            "texplosion",
            "mejecta",
            "shock_frac",
            "kappa",
            "radius_star"
        ],
        "replacements": {
            "luminosities": "luminosities_shock"
        }
    },
    "nickelcobalt": {
        "kind": "engine",
        "inputs": [
            "densetimes",
            "mejecta",
            "fnickel",
            "texplosion"
        ],
        "replacements": {
            "luminosities": "luminosities_nico"
        }
    },
    "diffusion": {
        "kind": "transform",
        "inputs": [
            "nickelcobalt",
            "kappa",
            "kappagamma",
            "mejecta",
            "texplosion",
            "vejecta"
        ],
        "replacements": {
            "luminosities": "luminosities_nico"
        }
    },
    "diffusion1": {
        "kind": "transform",
        "class": "diffusion",
        "inputs": [
            "shock_cooling",
            "kappa",
            "kappagamma",
            "mejecta",
            "texplosion",
            "vejecta"
        ],
        "replacements": {
            "luminosities": "luminosities_shock"
        }
    },
    "total_luminosity": {
        "kind": "utility",
        "operator": "+",
        "operands": [
            "luminosities_shock",
            "luminosities_nico"
        ],
        "inputs": [
            "diffusion",
            "diffusion1"
        ],
        "result": "luminosities",
        "wants_dense": true,
        "class": "operator"
    },
    "temperature_floor": {
        "kind": "photosphere",
        "inputs": [
            "texplosion",
            "total_luminosity",
            "temperature"
        ],
        "replacements": {
            "luminosities": "luminosities"
        }
    },
    "blackbody": {
        "kind": "sed",
        "inputs": [
            "texplosion",
            "redshift",
            "temperature_floor"
        ],
        "requests": {
            "band_wave_ranges": "photometry"
        }
    },
    "losextinction": {
        "kind": "sed",
        "inputs": [
            "blackbody",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests": {
            "band_wave_ranges": "photometry"
        },
        "replacements": {
            "luminosities": "luminosities_nico"
        }
    },
    "photometry": {
        "kind": "observable",
        "supports": [
            "ultraviolet",
            "optical",
            "infrared"
        ],
        "inputs": [
            "losextinction",
            "lumdist"
        ],
        "replacements": {
            "luminosities": "luminosities_nico"
        }
    }
}